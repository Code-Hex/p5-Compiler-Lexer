          {
            'end_line' => 6,
            'has_warnings' => 0,
            'block_id' => 1,
            'token_num' => 3,
            'src' => ' require \'./test.pl\' ;',
            'indent' => 1,
            'start_line' => 6
          },
          {
            'end_line' => 7,
            'has_warnings' => 1,
            'block_id' => 1,
            'token_num' => 9,
            'src' => ' skip_all ( \'cygwin specific test\' ) unless $^O eq \'cygwin\' ;',
            'indent' => 1,
            'start_line' => 7
          },
          {
            'end_line' => 10,
            'has_warnings' => 1,
            'block_id' => 0,
            'token_num' => 7,
            'indent' => 0,
            'src' => ' plan ( tests => 16 ) ;',
