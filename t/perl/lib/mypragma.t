            'start_line' => 33,
            'block_id' => 3,
            'indent' => 1
          },
          {
            'end_line' => 34,
            'has_warnings' => 1,
            'start_line' => 34,
            'block_id' => 4,
            'indent' => 2,
            'token_num' => 3,
            'src' => ' no mypragma ;'
          },
          {
            'src' => ' is ( mypragma::in_effect ( ) , 0 , "pragma no longer in effect" ) ;',
            'token_num' => 11,
            'start_line' => 35,
            'block_id' => 4,
            'indent' => 2,
