            'block_id' => 1
          },
          {
            'has_warnings' => 0,
            'end_line' => 6,
            'token_num' => 7,
            'src' => ' @INC = qw(lib ../lib) ;',
            'start_line' => 6,
            'block_id' => 1,
            'indent' => 1
          },
          {
            'has_warnings' => 1,
            'end_line' => 7,
            'token_num' => 7,
            'src' => ' plan ( tests => 14 ) ;',
            'block_id' => 1,
            'start_line' => 7,
            'indent' => 1
          },
          {
            'end_line' => 10,
            'has_warnings' => 0,
            'block_id' => 0,
            'start_line' => 10,
            'indent' => 0,
            'src' => ' use strict ;',
            'token_num' => 3
          },
          {
            'src' => ' use warnings ;',
            'token_num' => 3,
            'start_line' => 11,
            'block_id' => 0,
            'indent' => 0,
            'has_warnings' => 0,
            'end_line' => 11
          },
          {
            'block_id' => 0,
            'start_line' => 13,
            'indent' => 0,
            'src' => ' use mypragma ( ) ;',
            'token_num' => 5,
            'end_line' => 13,
            'has_warnings' => 0
          },
          {
            'end_line' => 16,
            'has_warnings' => 1,
            'start_line' => 16,
            'indent' => 1,
            'block_id' => 2,
            'src' => ' is ( $^H { mypragma } , undef , "Shouldn\'t be in %^H yet" ) ;',
            'token_num' => 12
          },
          {
            'src' => ' is ( mypragma::in_effect ( ) , undef , "pragma not in effect yet" ) ;',
            'token_num' => 11,
            'start_line' => 19,
