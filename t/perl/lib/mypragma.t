            'has_warnings' => 1,
            'start_line' => 40,
            'block_id' => 3,
            'indent' => 1,
            'token_num' => 11,
            'src' => ' is ( mypragma::in_effect ( ) , 42 , "pragma is in effect within this block" ) ;'
          },
          {
            'token_num' => 9,
            'src' => ' eval qq{is(mypragma::in_effect(), 42,
	       "pragma is in effect within this eval"); 1} or die $@ ;',
            'indent' => 1,
            'start_line' => 41,
            'block_id' => 3,
            'has_warnings' => 0,
            'end_line' => 42
          },
          {
