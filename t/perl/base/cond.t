subtest 'get_groups_by_syntax_level' => sub {
    my $lexer = Compiler::Lexer->new('');
    my $tokens = $lexer->tokenize($script);
    my $stmts = $lexer->get_groups_by_syntax_level($tokens, Compiler::Lexer::SyntaxType::T_Stmt);
    is_deeply($stmts, [
          {
            'src' => ' print "1..4\\n" ;',
            'token_num' => 3,
            'start_line' => 5,
            'block_id' => 0,
            'indent' => 0,
            'has_warnings' => 0,
            'end_line' => 5
          },
          {
            'block_id' => 0,
            'start_line' => 7,
            'indent' => 0,
            'token_num' => 4,
            'src' => ' $x = \'0\' ;',
            'end_line' => 7,
            'has_warnings' => 1
          },
          {
            'token_num' => 9,
            'src' => ' $x eq $x && ( print "ok 1\\n" ) ;',
            'indent' => 0,
            'start_line' => 9,
            'block_id' => 0,
            'has_warnings' => 1,
            'end_line' => 9
          },
          {
            'has_warnings' => 1,
            'end_line' => 10,
            'token_num' => 9,
            'src' => ' $x ne $x && ( print "not ok 1\\n" ) ;',
            'block_id' => 0,
            'start_line' => 10,
            'indent' => 0
          },
          {
            'end_line' => 11,
            'has_warnings' => 1,
            'start_line' => 11,
            'block_id' => 0,
            'indent' => 0,
            'token_num' => 9,
            'src' => ' $x eq $x || ( print "not ok 2\\n" ) ;'
          },
          {
            'has_warnings' => 1,
            'end_line' => 12,
            'token_num' => 9,
            'src' => ' $x ne $x || ( print "ok 2\\n" ) ;',
            'indent' => 0,
            'start_line' => 12,
            'block_id' => 0
          },
          {
            'end_line' => 14,
            'has_warnings' => 1,
            'indent' => 0,
            'start_line' => 14,
            'block_id' => 0,
            'src' => ' $x == $x && ( print "ok 3\\n" ) ;',
            'token_num' => 9
          },
          {
            'has_warnings' => 1,
            'end_line' => 15,
            'token_num' => 9,
            'src' => ' $x != $x && ( print "not ok 3\\n" ) ;',
            'start_line' => 15,
            'indent' => 0,
            'block_id' => 0
          },
          {
            'has_warnings' => 1,
            'end_line' => 16,
            'src' => ' $x == $x || ( print "not ok 4\\n" ) ;',
            'token_num' => 9,
            'start_line' => 16,
            'indent' => 0,
            'block_id' => 0
          },
          {
            'has_warnings' => 1,
            'end_line' => 17,
            'src' => ' $x != $x || ( print "ok 4\\n" ) ;',
            'token_num' => 9,
            'start_line' => 17,
            'indent' => 0,
            'block_id' => 0
          }
        ]
, 'Compiler::Lexer::get_groups_by_syntax_level');
};

