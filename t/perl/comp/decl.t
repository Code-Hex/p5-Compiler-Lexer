subtest 'get_groups_by_syntax_level' => sub {
    my $lexer = Compiler::Lexer->new('');
    my $tokens = $lexer->tokenize($script);
    my $stmts = $lexer->get_groups_by_syntax_level($tokens, Compiler::Lexer::SyntaxType::T_Stmt);
    is_deeply($stmts, [
          {
            'block_id' => 0,
            'token_num' => 7,
            'end_line' => 7,
            'has_warnings' => 0,
            'indent' => 0,
            'src' => ' sub one { print "ok 1\\n" ; }',
            'start_line' => 5
          },
          {
            'indent' => 1,
            'src' => ' print "ok 1\\n" ;',
            'start_line' => 6,
            'has_warnings' => 0,
            'end_line' => 6,
            'token_num' => 3,
            'block_id' => 1
          },
          {
            'start_line' => 8,
            'indent' => 0,
            'src' => ' format one = ok 6
 . print "1..9\\n" ;',
            'token_num' => 8,
            'block_id' => 0,
            'has_warnings' => 0,
            'end_line' => 12
          },
          {
            'has_warnings' => 0,
            'end_line' => 14,
            'token_num' => 4,
            'block_id' => 0,
            'start_line' => 14,
            'src' => ' one ( ) ;',
            'indent' => 0
          },
          {
            'token_num' => 4,
            'block_id' => 0,
            'end_line' => 15,
            'has_warnings' => 1,
            'src' => ' two ( ) ;',
            'indent' => 0,
            'start_line' => 15
          },
          {
