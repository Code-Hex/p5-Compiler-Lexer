subtest 'get_groups_by_syntax_level' => sub {
    my $lexer = Compiler::Lexer->new('');
    my $tokens = $lexer->tokenize($script);
    my $stmts = $lexer->get_groups_by_syntax_level($tokens, Compiler::Lexer::SyntaxType::T_Stmt);
    is_deeply($stmts, [
          {
            'start_line' => 3,
            'indent' => 0,
            'src' => ' print "1..7\\n" ;',
            'end_line' => 3,
            'has_warnings' => 0,
            'block_id' => 0,
            'token_num' => 3
          },
          {
            'indent' => 0,
            'src' => ' $main::result = "" ;',
            'start_line' => 5,
            'has_warnings' => 1,
            'end_line' => 5,
            'block_id' => 0,
            'token_num' => 4
          },
          {
            'start_line' => 6,
            'indent' => 0,
            'src' => ' eval q{
    $main::result .= "a(".__PACKAGE__."/".eval("__PACKAGE__").")";
    package Foo {
	$main::result .= "b(".__PACKAGE__."/".eval("__PACKAGE__").")";
	package Bar::Baz {
	    $main::result .= "c(".__PACKAGE__."/".eval("__PACKAGE__").")";
	}
	$main::result .= "d(".__PACKAGE__."/".eval("__PACKAGE__").")";
    }
    $main::result .= "e(".__PACKAGE__."/".eval("__PACKAGE__").")";
} ;',
            'end_line' => 17,
            'has_warnings' => 0,
            'token_num' => 6,
            'block_id' => 0
          },
          {
            'start_line' => 18,
            'indent' => 0,
            'src' => ' print $main::result eq "a(main/main)b(Foo/Foo)c(Bar::Baz/Bar::Baz)d(Foo/Foo)e(main/main)" ? "ok 1\\n" : "not ok 1\\n" ;',
            'end_line' => 20,
            'has_warnings' => 1,
            'token_num' => 9,
            'block_id' => 0
          },
          {
            'src' => ' $main::result = "" ;',
            'indent' => 0,
            'start_line' => 22,
            'block_id' => 0,
            'token_num' => 4,
            'end_line' => 22,
            'has_warnings' => 1
          },
          {
            'token_num' => 6,
