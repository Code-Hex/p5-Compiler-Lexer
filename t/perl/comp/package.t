subtest 'get_groups_by_syntax_level' => sub {
    my $lexer = Compiler::Lexer->new('');
    my $tokens = $lexer->tokenize($script);
    my $stmts = $lexer->get_groups_by_syntax_level($tokens, Compiler::Lexer::SyntaxType::T_Stmt);
    is_deeply($stmts, [
          {
            'end_line' => 3,
            'has_warnings' => 0,
            'start_line' => 3,
            'block_id' => 0,
            'indent' => 0,
            'src' => ' print "1..14\\n" ;',
            'token_num' => 3
          },
          {
            'end_line' => 5,
            'has_warnings' => 1,
            'indent' => 0,
            'start_line' => 5,
            'block_id' => 0,
            'token_num' => 4,
            'src' => ' $blurfl = 123 ;'
          },
          {
            'src' => ' $foo = 3 ;',
            'token_num' => 4,
            'block_id' => 0,
            'start_line' => 6,
            'indent' => 0,
            'has_warnings' => 1,
            'end_line' => 6
          },
          {
            'token_num' => 3,
            'src' => ' package xyz ;',
            'start_line' => 8,
            'block_id' => 0,
            'indent' => 0,
            'has_warnings' => 1,
            'end_line' => 8
          },
          {
            'block_id' => 0,
            'start_line' => 10,
            'indent' => 0,
            'src' => ' sub new { bless [ ] ; }',
            'token_num' => 8,
            'end_line' => 10,
            'has_warnings' => 0
          },
          {
