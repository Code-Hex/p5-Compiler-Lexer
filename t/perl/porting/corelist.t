subtest 'get_groups_by_syntax_level' => sub {
    my $lexer = Compiler::Lexer->new('');
    my $tokens = $lexer->tokenize($script);
    my $stmts = $lexer->get_groups_by_syntax_level($tokens, Compiler::Lexer::SyntaxType::T_Stmt);
    is_deeply($stmts, [
          {
            'indent' => 0,
            'src' => ' use TestInit qw(T) ;',
            'start_line' => 5,
            'token_num' => 7,
            'block_id' => 0,
            'end_line' => 5,
            'has_warnings' => 0
          },
          {
            'has_warnings' => 0,
            'end_line' => 6,
            'block_id' => 0,
            'token_num' => 3,
            'start_line' => 6,
            'indent' => 0,
            'src' => ' use strict ;'
          },
          {
            'start_line' => 7,
            'indent' => 0,
            'src' => ' use Config ;',
            'token_num' => 3,
            'block_id' => 0,
            'end_line' => 7,
            'has_warnings' => 0
          },
          {
            'end_line' => 9,
            'has_warnings' => 0,
            'block_id' => 0,
            'token_num' => 3,
            'indent' => 0,
            'src' => ' require \'t/test.pl\' ;',
            'start_line' => 9
          },
          {
            'has_warnings' => 1,
            'end_line' => 11,
            'block_id' => 0,
            'token_num' => 7,
            'start_line' => 11,
            'src' => ' plan ( tests => 6 ) ;',
            'indent' => 0
          },
          {
            'src' => ' use_ok ( \'Module::CoreList\' ) ;',
            'indent' => 0,
            'start_line' => 13,
            'block_id' => 0,
            'token_num' => 5,
            'end_line' => 13,
            'has_warnings' => 1
          },
          {
            'start_line' => 14,
            'src' => ' use_ok ( \'Module::CoreList::Utils\' ) ;',
            'indent' => 0,
            'has_warnings' => 1,
            'end_line' => 14,
            'block_id' => 0,
            'token_num' => 5
          },
          {
            'src' => ' use_ok ( \'Module::CoreList::TieHashDelta\' ) ;',
            'indent' => 0,
            'start_line' => 15,
            'end_line' => 15,
            'has_warnings' => 1,
            'block_id' => 0,
            'token_num' => 5
          },
          {
            'start_line' => 17,
            'src' => ' { no warnings \'once\' ; ok ( defined $Module::CoreList::released { $] } , "$] exists in released" ) ; ok ( defined $Module::CoreList::version { $] } , "$] exists in version" ) ; ok ( defined $Module::CoreList::Utils::utilities { $] } , "$] exists in Utils" ) ; }',
            'indent' => 0,
            'has_warnings' => 1,
            'end_line' => 22,
            'token_num' => 39,
            'block_id' => 0
          },
          {
            'end_line' => 18,
            'has_warnings' => 1,
            'token_num' => 4,
            'block_id' => 1,
            'start_line' => 18,
            'indent' => 1,
            'src' => ' no warnings \'once\' ;'
          },
          {
            'block_id' => 1,
            'token_num' => 11,
            'has_warnings' => 1,
            'end_line' => 19,
            'start_line' => 19,
            'indent' => 1,
            'src' => ' ok ( defined $Module::CoreList::released { $] } , "$] exists in released" ) ;'
          },
          {
            'start_line' => 20,
            'src' => ' ok ( defined $Module::CoreList::version { $] } , "$] exists in version" ) ;',
            'indent' => 1,
            'end_line' => 20,
            'has_warnings' => 1,
            'token_num' => 11,
            'block_id' => 1
          },
          {
            'start_line' => 21,
