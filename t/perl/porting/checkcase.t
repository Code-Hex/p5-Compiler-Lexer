subtest 'get_groups_by_syntax_level' => sub {
    my $lexer = Compiler::Lexer->new('');
    my $tokens = $lexer->tokenize($script);
    my $stmts = $lexer->get_groups_by_syntax_level($$tokens, Compiler::Lexer::SyntaxType::T_Stmt);
    is_deeply($$stmts, [
          {
            'token_num' => 3,
            'has_warnings' => 0,
            'end_line' => 5,
            'src' => ' use warnings ;',
            'start_line' => 5,
            'indent' => 0,
            'block_id' => 0
          },
          {
            'token_num' => 3,
            'has_warnings' => 0,
            'end_line' => 6,
            'src' => ' use strict ;',
            'start_line' => 6,
            'indent' => 0,
            'block_id' => 0
          },
          {
            'token_num' => 3,
            'has_warnings' => 1,
            'end_line' => 7,
            'src' => ' use File::Find ;',
            'start_line' => 7,
            'indent' => 0,
            'block_id' => 0
          },
          {
            'token_num' => 3,
            'has_warnings' => 0,
            'end_line' => 9,
            'src' => ' my %files ;',
            'start_line' => 9,
            'indent' => 0,
            'block_id' => 0
          },
          {
            'token_num' => 5,
            'has_warnings' => 0,
            'end_line' => 10,
            'src' => ' my $test_count = 0 ;',
            'start_line' => 10,
            'indent' => 0,
            'block_id' => 0
          },
          {
            'token_num' => 48,
            'has_warnings' => 1,
            'end_line' => 25,
            'src' => ' find ( sub { return if -d $File::Find::name ; my $name = $File::Find::name ; $name =~ s/^\\.\\..// ; return if $name =~ m!\\A(?:x2p/)?[Mm]akefile\\z! ; push @{ $files { lc $name } } , $name ; } , \'..\' ) ;',
            'start_line' => 12,
            'indent' => 0,
            'block_id' => 0
          },
          {
            'token_num' => 5,
            'has_warnings' => 1,
            'end_line' => 15,
            'src' => ' return if -d $File::Find::name ;',
            'start_line' => 15,
            'indent' => 1,
            'block_id' => 1
          },
          {
            'token_num' => 5,
            'has_warnings' => 1,
            'end_line' => 17,
            'src' => ' my $name = $File::Find::name ;',
            'start_line' => 17,
            'indent' => 1,
            'block_id' => 1
          },
          {
            'token_num' => 9,
            'has_warnings' => 1,
            'end_line' => 19,
            'src' => ' $name =~ s/^\\.\\..// ;',
            'start_line' => 19,
            'indent' => 1,
            'block_id' => 1
          },
          {
            'token_num' => 9,
            'has_warnings' => 1,
            'end_line' => 22,
            'src' => ' return if $name =~ m!\\A(?:x2p/)?[Mm]akefile\\z! ;',
            'start_line' => 22,
            'indent' => 1,
            'block_id' => 1
          },
          {
            'token_num' => 11,
            'has_warnings' => 1,
            'end_line' => 24,
            'src' => ' push @{ $files { lc $name } } , $name ;',
            'start_line' => 24,
            'indent' => 1,
            'block_id' => 1
          },
          {
            'token_num' => 59,
            'has_warnings' => 1,
            'end_line' => 34,
            'src' => ' foreach ( sort values %files ) { if ( @$_ > 1 ) { print "not ok " . ++ $test_count . " - " . join ( ", " , @$_ ) , "\\n" ; print STDERR "# $_\\n" foreach @$_ ; } else { print "ok " . ++ $test_count . " - " . join ( ", " , @$_ ) , "\\n" ; } }',
            'start_line' => 27,
            'indent' => 0,
            'block_id' => 0
          },
          {
            'token_num' => 31,
            'has_warnings' => 1,
            'end_line' => 31,
            'src' => ' if ( @$_ > 1 ) { print "not ok " . ++ $test_count . " - " . join ( ", " , @$_ ) , "\\n" ; print STDERR "# $_\\n" foreach @$_ ; }',
            'start_line' => 28,
            'indent' => 1,
            'block_id' => 2
          },
          {
            'token_num' => 17,
            'has_warnings' => 1,
            'end_line' => 29,
            'src' => ' print "not ok " . ++ $test_count . " - " . join ( ", " , @$_ ) , "\\n" ;',
            'start_line' => 29,
            'indent' => 2,
            'block_id' => 3
          },
          {
            'token_num' => 6,
            'has_warnings' => 0,
            'end_line' => 30,
            'src' => ' print STDERR "# $_\\n" foreach @$_ ;',
            'start_line' => 30,
            'indent' => 2,
            'block_id' => 3
          },
          {
            'token_num' => 20,
            'has_warnings' => 1,
            'end_line' => 33,
            'src' => ' else { print "ok " . ++ $test_count . " - " . join ( ", " , @$_ ) , "\\n" ; }',
            'start_line' => 31,
            'indent' => 1,
            'block_id' => 2
          },
          {
            'token_num' => 17,
            'has_warnings' => 1,
            'end_line' => 32,
            'src' => ' print "ok " . ++ $test_count . " - " . join ( ", " , @$_ ) , "\\n" ;',
            'start_line' => 32,
            'indent' => 2,
            'block_id' => 4
          },
          {
            'token_num' => 7,
            'has_warnings' => 1,
            'end_line' => 36,
            'src' => ' print "1.." . $test_count . "\\n" ;',
            'start_line' => 36,
            'indent' => 0,
            'block_id' => 0
          }
        ]
, 'Compiler::Lexer::get_groups_by_syntax_level');
};

subtest 'get_used_modules' => sub {
    my $modules = Compiler::Lexer->new('')->get_used_modules($script);
    is_deeply($modules, [
          {
            'args' => '',
            'name' => 'warnings'
          },
          {
            'args' => '',
            'name' => 'strict'
          },
          {
            'args' => '',
            'name' => 'File::Find'
          }
        ]
, 'Compiler::Lexer::get_used_modules');
};

