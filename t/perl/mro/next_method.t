subtest 'get_used_modules' => sub {
    my $modules = Compiler::Lexer->new('')->get_used_modules($script);
    is_deeply($modules, [
          {
            'args' => '',
            'name' => 'strict'
          },
          {
            'name' => 'warnings',
            'args' => ''
          },
          {
            'name' => 'mro',
            'args' => '  \'c3\''
          },
          {
            'name' => 'base',
            'args' => '  \'Diamond_A\''
          },
          {
            'name' => 'mro',
            'args' => '  \'c3\''
          },
          {
            'args' => '  \'c3\'',
            'name' => 'mro'
          },
          {
            'args' => '  \'Diamond_A\'',
            'name' => 'base'
          },
          {
            'name' => 'base',
            'args' => '  ( \'Diamond_B\' , \'Diamond_C\' )'
          },
          {
            'args' => '  \'c3\'',
            'name' => 'mro'
          }
        ]
, 'Compiler::Lexer::get_used_modules');
};

