subtest 'get_used_modules' => sub {
    my $modules = Compiler::Lexer->new('')->get_used_modules($script);
    is_deeply($modules, [
          {
            'name' => 'strict',
            'args' => ''
          },
          {
            'args' => '',
            'name' => 'warnings'
          },
          {
            'name' => 'base',
            'args' => '  \'Diamond_A\''
          },
          {
            'name' => 'base',
            'args' => '  \'Diamond_A\''
          },
          {
            'args' => '  ( \'Diamond_B\' , \'Diamond_C\' )',
            'name' => 'base'
          },
          {
            'name' => 'mro',
            'args' => '  \'c3\''
          }
        ]
, 'Compiler::Lexer::get_used_modules');
};

