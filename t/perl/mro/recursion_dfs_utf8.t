          {
            'args' => '',
            'name' => 'strict'
          },
          {
            'name' => 'warnings',
            'args' => ''
          },
          {
            'args' => '',
            'name' => 'utf8'
          },
          {
            'name' => 'open',
            'args' => '  qw (  :utf8 :std  )'
          }
        ]
, 'Compiler::Lexer::get_used_modules');
};

