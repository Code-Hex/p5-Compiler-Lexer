subtest 'get_groups_by_syntax_level' => sub {
    my $lexer = Compiler::Lexer->new('');
    my $tokens = $lexer->tokenize($script);
    my $stmts = $lexer->get_groups_by_syntax_level($tokens, Compiler::Lexer::SyntaxType::T_Stmt);
    is_deeply($stmts, [
          {
            'token_num' => 3,
            'src' => ' use strict ;',
            'block_id' => 0,
            'start_line' => 3,
            'indent' => 0,
            'has_warnings' => 0,
            'end_line' => 3
          },
          {
            'has_warnings' => 0,
            'end_line' => 4,
            'src' => ' use warnings ;',
            'token_num' => 3,
            'start_line' => 4,
            'indent' => 0,
            'block_id' => 0
          },
          {
            'src' => ' unless ( -d \'blib\' ) { chdir \'t\' if -d \'t\' ; @INC = \'../lib\' ; }',
            'token_num' => 17,
            'block_id' => 1,
            'start_line' => 6,
            'indent' => 1,
            'has_warnings' => 0,
            'end_line' => 9
          },
          {
            'indent' => 2,
            'start_line' => 7,
            'block_id' => 2,
            'token_num' => 6,
            'src' => ' chdir \'t\' if -d \'t\' ;',
            'end_line' => 7,
            'has_warnings' => 0
