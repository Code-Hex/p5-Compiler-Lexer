            'start_line' => 6,
            'block_id' => 2
          },
          {
            'has_warnings' => 0,
            'end_line' => 10,
            'src' => ' use strict ;',
            'token_num' => 3,
            'indent' => 0,
            'start_line' => 10,
            'block_id' => 0
          },
          {
            'has_warnings' => 0,
            'end_line' => 11,
            'src' => ' use warnings ;',
            'token_num' => 3,
            'indent' => 0,
            'start_line' => 11,
            'block_id' => 0
          },
          {
            'end_line' => 13,
            'has_warnings' => 0,
