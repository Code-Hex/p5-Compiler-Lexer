subtest 'get_groups_by_syntax_level' => sub {
    my $lexer = Compiler::Lexer->new('');
    my $tokens = $lexer->tokenize($script);
    my $stmts = $lexer->get_groups_by_syntax_level($tokens, Compiler::Lexer::SyntaxType::T_Stmt);
    is_deeply($stmts, [
          {
            'token_num' => 6,
            'src' => ' chdir \'t\' if -d \'t\' ;',
            'start_line' => 4,
            'block_id' => 1,
            'indent' => 1,
            'has_warnings' => 0,
            'end_line' => 4
          },
          {
            'start_line' => 5,
            'indent' => 1,
            'block_id' => 1,
            'src' => ' @INC = \'../lib\' ;',
            'token_num' => 4,
            'end_line' => 5,
            'has_warnings' => 0
          },
          {
            'indent' => 1,
            'start_line' => 6,
            'block_id' => 1,
            'src' => ' require "./test.pl" ;',
            'token_num' => 3,
            'end_line' => 6,
            'has_warnings' => 0
          },
          {
            'start_line' => 7,
            'block_id' => 1,
            'indent' => 1,
            'src' => ' eval \'use Errno\' ;',
            'token_num' => 3,
            'end_line' => 7,
            'has_warnings' => 0
          },
          {
            'has_warnings' => 1,
            'end_line' => 8,
            'src' => ' die $@ if $@ and ! is_miniperl ( ) ;',
            'token_num' => 10,
            'start_line' => 8,
            'block_id' => 1,
            'indent' => 1
          },
          {
            'end_line' => 11,
            'has_warnings' => 1,
            'start_line' => 11,
            'block_id' => 0,
            'indent' => 0,
            'src' => ' my $has_link = Win32::FsType ( ) eq \'NTFS\' ;',
            'token_num' => 9
          },
          {
            'has_warnings' => 1,
            'end_line' => 14,
            'token_num' => 7,
            'src' => ' $has_link or skip_all ( "link() only works on NTFS" ) ;',
            'start_line' => 14,
            'block_id' => 0,
            'indent' => 0
          },
          {
            'start_line' => 16,
            'block_id' => 0,
            'indent' => 0,
            'src' => ' plan tests => 4 ;',
            'token_num' => 5,
            'end_line' => 16,
            'has_warnings' => 1
          },
          {
            'start_line' => 18,
            'block_id' => 0,
            'indent' => 0,
            'src' => ' my $tmpfile1 = tempfile ( ) ;',
            'token_num' => 7,
            'end_line' => 18,
            'has_warnings' => 1
          },
          {
            'end_line' => 19,
            'has_warnings' => 1,
            'start_line' => 19,
            'block_id' => 0,
            'indent' => 0,
            'token_num' => 7,
            'src' => ' my $tmpfile2 = tempfile ( ) ;'
          },
          {
            'has_warnings' => 1,
            'end_line' => 23,
            'token_num' => 13,
            'src' => ' ok ( ! link ( $tmpfile1 , $tmpfile2 ) , "Cannot link to unknown file" ) ;',
            'indent' => 0,
            'start_line' => 22,
            'block_id' => 0
          },
          {
            'end_line' => 24,
            'has_warnings' => 1,
            'start_line' => 24,
            'block_id' => 0,
            'indent' => 0,
            'token_num' => 12,
            'src' => ' is ( 0 + $! , & Errno::ENOENT , "check errno is ENOENT" ) ;'
          },
          {
            'end_line' => 26,
