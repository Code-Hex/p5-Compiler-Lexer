            'src' => ' close $fh ;',
            'token_num' => 3,
            'end_line' => 27,
            'has_warnings' => 1
          },
          {
            'block_id' => 0,
            'start_line' => 28,
            'indent' => 0,
            'src' => ' open my $fh , ">" , $tmpfile2 or skip ( "Cannot create test link target" , 2 ) ;',
            'token_num' => 15,
            'end_line' => 29,
            'has_warnings' => 1
          },
          {
            'has_warnings' => 1,
            'end_line' => 30,
            'src' => ' close $fh ;',
            'token_num' => 3,
            'start_line' => 30,
            'block_id' => 0,
            'indent' => 0
          },
          {
            'start_line' => 31,
            'indent' => 0,
            'block_id' => 0,
            'token_num' => 13,
            'src' => ' ok ( ! link ( $tmpfile1 , $tmpfile2 ) , "Cannot link to existing file" ) ;',
            'end_line' => 32,
            'has_warnings' => 1
          },
          {
            'end_line' => 33,
            'has_warnings' => 1,
            'start_line' => 33,
            'block_id' => 0,
            'indent' => 0,
            'token_num' => 12,
            'src' => ' is ( 0 + $! , & Errno::EEXIST , "check for EEXIST" ) ;'
          }
        ]
, 'Compiler::Lexer::get_groups_by_syntax_level');
};

