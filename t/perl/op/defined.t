subtest 'get_groups_by_syntax_level' => sub {
    my $lexer = Compiler::Lexer->new('');
    my $tokens = $lexer->tokenize($script);
    my $stmts = $lexer->get_groups_by_syntax_level($tokens, Compiler::Lexer::SyntaxType::T_Stmt);
    is_deeply($stmts, [
          {
            'token_num' => 6,
            'block_id' => 1,
            'has_warnings' => 0,
            'end_line' => 3,
            'start_line' => 3,
            'src' => ' chdir \'t\' if -d \'t\' ;',
            'indent' => 1
          },
          {
            'src' => ' require \'./test.pl\' ;',
            'indent' => 1,
            'start_line' => 4,
            'end_line' => 4,
            'has_warnings' => 0,
            'token_num' => 3,
            'block_id' => 1
          },
          {
            'start_line' => 7,
            'src' => ' plan 5 ;',
            'indent' => 0,
            'block_id' => 0,
            'token_num' => 3,
            'has_warnings' => 1,
            'end_line' => 7
          },
          {
            'token_num' => 5,
            'block_id' => 0,
            'end_line' => 9,
            'has_warnings' => 0,
            'src' => ' sub notdef { undef }',
            'indent' => 0,
            'start_line' => 9
          },
          {
            'token_num' => 11,
            'block_id' => 0,
            'end_line' => 13,
            'has_warnings' => 1,
            'src' => ' ok ! defined do { & notdef } , \'defined do { &sub }\' ;',
            'indent' => 0,
            'start_line' => 13
          },
          {
            'token_num' => 5,
            'block_id' => 2,
            'end_line' => 13,
            'has_warnings' => 0,
            'indent' => 0,
            'src' => ' do { & notdef }',
            'start_line' => 13
          },
          {
            'token_num' => 15,
            'block_id' => 0,
            'end_line' => 14,
            'has_warnings' => 1,
            'src' => ' ok ! defined ( scalar ( 42 , & notdef ) ) , \'defined(scalar(42,&sub))\' ;',
            'indent' => 0,
            'start_line' => 14
          },
          {
            'src' => ' ok ! defined do { ( ) ; & notdef } , \'!defined do{();&sub}\' ;',
            'indent' => 0,
            'start_line' => 15,
            'block_id' => 0,
            'token_num' => 14,
            'end_line' => 15,
            'has_warnings' => 1
          },
          {
            'indent' => 0,
            'src' => ' do { ( ) ; & notdef }',
            'start_line' => 15,
            'block_id' => 3,
            'token_num' => 8,
            'has_warnings' => 0,
            'end_line' => 15
          },
          {
            'end_line' => 15,
            'has_warnings' => 0,
            'token_num' => 3,
            'block_id' => 4,
            'indent' => 1,
            'src' => ' ( ) ;',
            'start_line' => 15
          },
          {
            'src' => ' no warnings "deprecated" ;',
            'indent' => 0,
            'start_line' => 18,
            'token_num' => 4,
            'block_id' => 0,
            'has_warnings' => 1,
            'end_line' => 18
          },
          {
            'block_id' => 0,
            'token_num' => 12,
            'end_line' => 19,
            'has_warnings' => 1,
            'start_line' => 19,
            'indent' => 0,
            'src' => ' ok defined ( $false ? $scalar : @array ) , \'defined( ... ? ... : @array)\' ;'
          },
          {
            'indent' => 0,
            'src' => ' ok defined ( scalar @array ) , \'defined(scalar @array)\' ;',
            'start_line' => 20,
            'block_id' => 0,
            'token_num' => 9,
            'end_line' => 20,
            'has_warnings' => 1
