subtest 'get_groups_by_syntax_level' => sub {
    my $lexer = Compiler::Lexer->new('');
    my $tokens = $lexer->tokenize($script);
    my $stmts = $lexer->get_groups_by_syntax_level($tokens, Compiler::Lexer::SyntaxType::T_Stmt);
    is_deeply($stmts, [
          {
            'start_line' => 4,
            'indent' => 1,
            'src' => ' chdir \'t\' if -d \'t\' ;',
            'end_line' => 4,
            'has_warnings' => 0,
            'token_num' => 6,
            'block_id' => 1
          },
          {
            'block_id' => 1,
            'token_num' => 4,
            'end_line' => 5,
            'has_warnings' => 0,
            'start_line' => 5,
            'indent' => 1,
            'src' => ' @INC = \'../lib\' ;'
          },
          {
            'start_line' => 6,
            'src' => ' require \'./test.pl\' ;',
            'indent' => 1,
            'block_id' => 1,
            'token_num' => 3,
            'end_line' => 6,
            'has_warnings' => 0
          },
          {
            'start_line' => 9,
            'src' => ' plan tests => 48 ;',
