subtest 'get_groups_by_syntax_level' => sub {
    my $lexer = Compiler::Lexer->new('');
    my $tokens = $lexer->tokenize($script);
    my $stmts = $lexer->get_groups_by_syntax_level($tokens, Compiler::Lexer::SyntaxType::T_Stmt);
    is_deeply($stmts, [
          {
            'indent' => 1,
            'src' => ' chdir \'t\' if -d \'t\' ;',
            'start_line' => 4,
            'block_id' => 1,
            'token_num' => 6,
            'end_line' => 4,
            'has_warnings' => 0
          },
          {
            'src' => ' @INC = \'../lib\' ;',
            'indent' => 1,
            'start_line' => 5,
            'has_warnings' => 0,
            'end_line' => 5,
            'token_num' => 4,
            'block_id' => 1
          },
          {
            'start_line' => 6,
            'src' => ' require \'./test.pl\' ;',
            'indent' => 1,
            'has_warnings' => 0,
            'end_line' => 6,
            'token_num' => 3,
            'block_id' => 1
          },
          {
            'start_line' => 7,
            'indent' => 1,
            'src' => ' skip_all_if_miniperl ( "no dynamic loading on miniperl, no IO, hence no FileHandle" ) ;',
            'has_warnings' => 1,
            'end_line' => 7,
            'block_id' => 1,
            'token_num' => 5
          },
          {
            'src' => ' plan 4 ;',
            'indent' => 0,
            'start_line' => 10,
            'token_num' => 3,
            'block_id' => 0,
            'end_line' => 10,
            'has_warnings' => 1
          },
          {
            'block_id' => 0,
            'token_num' => 3,
            'end_line' => 11,
            'has_warnings' => 0,
            'src' => ' use FileHandle ;',
            'indent' => 0,
            'start_line' => 11
          },
          {
            'has_warnings' => 0,
            'end_line' => 13,
            'block_id' => 0,
            'token_num' => 5,
            'start_line' => 13,
            'src' => ' my $str = "foo" ;',
            'indent' => 0
          },
          {
            'token_num' => 9,
            'block_id' => 0,
            'has_warnings' => 1,
            'end_line' => 14,
            'indent' => 0,
            'src' => ' open my $fh , "<" , \\ $str ;',
            'start_line' => 14
          },
          {
            'token_num' => 9,
            'block_id' => 0,
            'has_warnings' => 1,
            'end_line' => 15,
            'indent' => 0,
            'src' => ' is < $fh > , "foo" , "open fh to reference to string: got expected content" ;',
            'start_line' => 15
          },
          {
            'has_warnings' => 1,
            'end_line' => 21,
            'block_id' => 0,
            'token_num' => 31,
            'start_line' => 17,
            'indent' => 0,
            'src' => ' eval { $fh-> seek ( 0 , 0 ) ; is $fh-> tell , 0 , "after \'seek\' and \'tell\', got expected current fh position in bytes" ; is < $fh > , "foo" , "after \'seek\' and \'tell\', still got expected content" ; } ;'
          },
          {
            'indent' => 0,
            'src' => ' $fh-> seek ( 0 , 0 ) ;',
            'start_line' => 18,
            'token_num' => 9,
            'block_id' => 1,
            'has_warnings' => 1,
            'end_line' => 18
          },
          {
            'has_warnings' => 1,
            'end_line' => 19,
            'block_id' => 1,
            'token_num' => 9,
            'start_line' => 19,
            'indent' => 0,
            'src' => ' is $fh-> tell , 0 , "after \'seek\' and \'tell\', got expected current fh position in bytes" ;'
          },
          {
            'end_line' => 20,
            'has_warnings' => 1,
            'token_num' => 9,
            'block_id' => 1,
            'indent' => 0,
            'src' => ' is < $fh > , "foo" , "after \'seek\' and \'tell\', still got expected content" ;',
            'start_line' => 20
          },
          {
            'indent' => 0,
            'src' => ' is $@ , \'\' , "no errors after \'seek\' or \'tell\'" ;',
            'start_line' => 23,
            'has_warnings' => 1,
            'end_line' => 23,
            'block_id' => 0,
            'token_num' => 7
          }
        ]
, 'Compiler::Lexer::get_groups_by_syntax_level');
};

