          {
            'args' => '  "uninitialized"',
            'name' => 'warnings'
          },
          {
            'name' => 'bytes',
            'args' => ''
          },
          {
            'name' => 'overload',
            'args' => '  q | "" |  =>  sub  { ${ $_ [ 0 ] } ++ }  }'
          }
        ]
, 'Compiler::Lexer::get_used_modules');
};

