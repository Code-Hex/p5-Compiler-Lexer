subtest 'get_groups_by_syntax_level' => sub {
    my $lexer = Compiler::Lexer->new('');
    my $tokens = $lexer->tokenize($script);
    my $stmts = $lexer->get_groups_by_syntax_level($tokens, Compiler::Lexer::SyntaxType::T_Stmt);
    is_deeply($stmts, [
          {
            'src' => ' chdir \'t\' if -d \'t\' ;',
            'token_num' => 6,
            'indent' => 1,
            'start_line' => 4,
            'block_id' => 1,
            'has_warnings' => 0,
            'end_line' => 4
          },
          {
            'start_line' => 5,
            'block_id' => 1,
            'indent' => 1,
            'src' => ' require \'./test.pl\' ;',
            'token_num' => 3,
            'end_line' => 5,
            'has_warnings' => 0
          },
          {
            'has_warnings' => 1,
            'end_line' => 6,
            'src' => ' skip_all_if_miniperl ( "no dynamic loading on miniperl, no File::Spec (used by charnames)" ) ;',
