subtest 'get_groups_by_syntax_level' => sub {
    my $lexer = Compiler::Lexer->new('');
    my $tokens = $lexer->tokenize($script);
    my $stmts = $lexer->get_groups_by_syntax_level($tokens, Compiler::Lexer::SyntaxType::T_Stmt);
    is_deeply($stmts, [
          {
            'end_line' => 4,
            'has_warnings' => 0,
            'start_line' => 4,
            'indent' => 1,
            'block_id' => 1,
            'token_num' => 6,
            'src' => ' chdir \'t\' if -d \'t\' ;'
          },
          {
            'indent' => 1,
            'start_line' => 5,
            'block_id' => 1,
            'token_num' => 4,
            'src' => ' @INC = \'../lib\' ;',
            'end_line' => 5,
            'has_warnings' => 0
          },
          {
            'end_line' => 6,
            'has_warnings' => 0,
            'start_line' => 6,
            'block_id' => 1,
            'indent' => 1,
            'token_num' => 3,
            'src' => ' require \'./test.pl\' ;'
          },
          {
            'has_warnings' => 0,
            'end_line' => 8,
            'src' => ' use strict ;',
            'token_num' => 3,
            'indent' => 0,
            'start_line' => 8,
            'block_id' => 0
          },
          {
            'end_line' => 9,
            'has_warnings' => 0,
            'start_line' => 9,
            'block_id' => 0,
            'indent' => 0,
            'src' => ' use warnings ;',
            'token_num' => 3
          },
          {
            'token_num' => 7,
            'src' => ' use vars qw(@array @r $k $v $c) ;',
            'block_id' => 0,
            'start_line' => 10,
            'indent' => 0,
            'has_warnings' => 0,
            'end_line' => 10
