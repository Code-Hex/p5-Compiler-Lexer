            'src' => ' $sub = sub : const { $x + 5 } ;'
          },
          {
            'has_warnings' => 1,
            'end_line' => 31,
            'src' => ' $x ++ ;',
            'token_num' => 3,
            'indent' => 0,
            'start_line' => 31,
            'block_id' => 0
          },
          {
            'start_line' => 32,
            'block_id' => 0,
            'indent' => 0,
            'src' => ' is &$sub , 8 , \':const capturing expression\' ;',
            'token_num' => 7,
            'end_line' => 32,
            'has_warnings' => 1
          }
        ]
, 'Compiler::Lexer::get_groups_by_syntax_level');
};

