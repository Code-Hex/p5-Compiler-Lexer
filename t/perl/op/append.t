subtest 'get_groups_by_syntax_level' => sub {
    my $lexer = Compiler::Lexer->new('');
    my $tokens = $lexer->tokenize($script);
    my $stmts = $lexer->get_groups_by_syntax_level($tokens, Compiler::Lexer::SyntaxType::T_Stmt);
    is_deeply($stmts, [
          {
            'end_line' => 4,
            'has_warnings' => 0,
            'indent' => 1,
            'start_line' => 4,
            'block_id' => 1,
            'src' => ' chdir \'t\' if -d \'t\' ;',
            'token_num' => 6
          },
          {
            'start_line' => 5,
            'indent' => 1,
            'block_id' => 1,
            'token_num' => 4,
            'src' => ' @INC = \'../lib\' ;',
            'end_line' => 5,
            'has_warnings' => 0
          },
          {
            'token_num' => 3,
            'src' => ' require \'./test.pl\' ;',
            'block_id' => 1,
            'start_line' => 6,
            'indent' => 1,
            'has_warnings' => 0,
            'end_line' => 6
          },
          {
            'start_line' => 10,
            'block_id' => 0,
            'indent' => 0,
            'src' => ' plan tests => 13 ;',
            'token_num' => 5,
            'end_line' => 10,
            'has_warnings' => 1
          },
