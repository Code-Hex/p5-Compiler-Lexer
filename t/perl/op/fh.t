subtest 'get_groups_by_syntax_level' => sub {
    my $lexer = Compiler::Lexer->new('');
    my $tokens = $lexer->tokenize($script);
    my $stmts = $lexer->get_groups_by_syntax_level($tokens, Compiler::Lexer::SyntaxType::T_Stmt);
    is_deeply($stmts, [
          {
            'token_num' => 6,
            'block_id' => 1,
            'has_warnings' => 0,
            'end_line' => 4,
            'start_line' => 4,
            'src' => ' chdir \'t\' if -d \'t\' ;',
            'indent' => 1
          },
          {
            'has_warnings' => 0,
            'end_line' => 5,
            'block_id' => 1,
            'token_num' => 4,
            'start_line' => 5,
            'src' => ' @INC = \'../lib\' ;',
            'indent' => 1
          },
          {
            'token_num' => 3,
            'block_id' => 1,
            'end_line' => 6,
            'has_warnings' => 0,
            'indent' => 1,
            'src' => ' require \'./test.pl\' ;',
            'start_line' => 6
          },
          {
            'token_num' => 5,
            'block_id' => 0,
            'end_line' => 9,
            'has_warnings' => 1,
            'start_line' => 9,
            'src' => ' plan tests => 8 ;',
            'indent' => 0
          },
          {
            'has_warnings' => 0,
