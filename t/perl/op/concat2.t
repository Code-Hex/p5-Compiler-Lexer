subtest 'get_groups_by_syntax_level' => sub {
    my $lexer = Compiler::Lexer->new('');
    my $tokens = $lexer->tokenize($script);
    my $stmts = $lexer->get_groups_by_syntax_level($tokens, Compiler::Lexer::SyntaxType::T_Stmt);
    is_deeply($stmts, [
          {
            'token_num' => 6,
            'src' => ' chdir \'t\' if -d \'t\' ;',
            'start_line' => 10,
            'block_id' => 1,
            'indent' => 1,
            'has_warnings' => 0,
            'end_line' => 10
          },
          {
            'has_warnings' => 0,
            'end_line' => 11,
            'token_num' => 4,
            'src' => ' @INC = \'../lib\' ;',
            'indent' => 1,
            'start_line' => 11,
            'block_id' => 1
