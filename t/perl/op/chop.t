subtest 'get_groups_by_syntax_level' => sub {
    my $lexer = Compiler::Lexer->new('');
    my $tokens = $lexer->tokenize($script);
    my $stmts = $lexer->get_groups_by_syntax_level($tokens, Compiler::Lexer::SyntaxType::T_Stmt);
    is_deeply($stmts, [
          {
            'indent' => 1,
            'start_line' => 4,
            'block_id' => 1,
            'token_num' => 6,
            'src' => ' chdir \'t\' if -d \'t\' ;',
            'end_line' => 4,
            'has_warnings' => 0
          },
          {
            'start_line' => 5,
            'block_id' => 1,
            'indent' => 1,
            'src' => ' @INC = \'../lib\' ;',
            'token_num' => 4,
            'end_line' => 5,
            'has_warnings' => 0
          },
          {
            'src' => ' require \'./test.pl\' ;',
