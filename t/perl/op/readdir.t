subtest 'get_groups_by_syntax_level' => sub {
    my $lexer = Compiler::Lexer->new('');
    my $tokens = $lexer->tokenize($script);
    my $stmts = $lexer->get_groups_by_syntax_level($tokens, Compiler::Lexer::SyntaxType::T_Stmt);
    is_deeply($stmts, [
          {
            'indent' => 1,
            'start_line' => 4,
            'block_id' => 1,
            'token_num' => 6,
            'src' => ' chdir \'t\' if -d \'t\' ;',
            'end_line' => 4,
            'has_warnings' => 0
          },
          {
            'start_line' => 5,
            'block_id' => 1,
            'indent' => 1,
            'token_num' => 4,
            'src' => ' @INC = \'../lib\' ;',
            'end_line' => 5,
            'has_warnings' => 0
          },
          {
            'has_warnings' => 0,
            'end_line' => 6,
            'token_num' => 3,
            'src' => ' require \'./test.pl\' ;',
            'start_line' => 6,
            'block_id' => 1,
            'indent' => 1
          },
          {
            'token_num' => 3,
            'src' => ' use strict ;',
            'block_id' => 0,
            'start_line' => 9,
            'indent' => 0,
            'has_warnings' => 0,
            'end_line' => 9
          },
          {
            'end_line' => 10,
            'has_warnings' => 0,
            'block_id' => 0,
            'start_line' => 10,
            'indent' => 0,
            'src' => ' use warnings ;',
            'token_num' => 3
          },
          {
            'has_warnings' => 0,
            'end_line' => 11,
            'src' => ' use vars qw($fh @fh %fh) ;',
            'token_num' => 7,
            'block_id' => 0,
            'start_line' => 11,
            'indent' => 0
          },
          {
            'has_warnings' => 0,
            'end_line' => 13,
            'token_num' => 3,
            'src' => ' eval \'opendir(NOSUCH, "no/such/directory");\' ;',
            'start_line' => 13,
            'indent' => 0,
            'block_id' => 0
          },
          {
            'has_warnings' => 1,
            'end_line' => 14,
            'src' => ' skip_all ( $@ ) if $@ ;',
            'token_num' => 7,
            'start_line' => 14,
            'indent' => 0,
            'block_id' => 0
          },
          {
            'src' => ' for my $i ( 1 .. 2000 ) { local *OP ; opendir ( OP , "op" ) or die "can\'t opendir: $!" ; }',
            'token_num' => 23,
            'start_line' => 16,
            'indent' => 0,
            'block_id' => 0,
            'has_warnings' => 1,
            'end_line' => 20
          },
          {
            'indent' => 1,
            'start_line' => 17,
            'block_id' => 2,
            'src' => ' local *OP ;',
            'token_num' => 3,
            'end_line' => 17,
            'has_warnings' => 0
          },
