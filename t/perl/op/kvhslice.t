            'token_num' => 3
          },
          {
            'end_line' => 6,
            'has_warnings' => 1,
            'block_id' => 1,
            'token_num' => 5,
            'src' => ' set_up_inc ( \'../lib\' ) ;',
            'indent' => 1,
            'start_line' => 6
          },
          {
            'end_line' => 11,
            'has_warnings' => 1,
            'token_num' => 5,
            'block_id' => 0,
            'start_line' => 11,
            'indent' => 0,
            'src' => ' plan tests => 39 ;'
          },
          {
            'src' => ' { my %h = map { $_ => uc $_ } \'a\' .. \'z\' ; is ( join ( \':\' , %h { \'c\' , \'d\' , \'e\' } ) , \'c:C:d:D:e:E\' , "correct result and order" ) ; is ( join ( \':\' , %h { \'e\' , \'d\' , \'c\' } ) , \'e:E:d:D:c:C\' , "correct result and order" ) ; is ( join ( \':\' , %h { \'e\' , \'c\' , \'d\' } ) , \'e:E:c:C:d:D\' , "correct result and order" ) ; ok ( eq_hash ( { %h { \'q\' , \'w\' } } , { q => \'Q\' , w => \'W\' } ) , "correct hash" ) ; is ( join ( \':\' , %h { ( ) } ) , \'\' , "correct result for empty slice" ) ; }',
            'indent' => 0,
            'start_line' => 14,
            'end_line' => 24,
            'has_warnings' => 1,
            'block_id' => 0,
            'token_num' => 124
          },
          {
            'indent' => 1,
