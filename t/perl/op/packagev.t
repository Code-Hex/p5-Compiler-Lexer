subtest 'get_groups_by_syntax_level' => sub {
    my $lexer = Compiler::Lexer->new('');
    my $tokens = $lexer->tokenize($script);
    my $stmts = $lexer->get_groups_by_syntax_level($tokens, Compiler::Lexer::SyntaxType::T_Stmt);
    is_deeply($stmts, [
          {
            'indent' => 1,
            'src' => ' chdir \'t\' if -d \'t\' ;',
            'start_line' => 4,
            'token_num' => 6,
            'block_id' => 1,
            'has_warnings' => 0,
            'end_line' => 4
          },
          {
            'indent' => 1,
            'src' => ' @INC = qw \'../lib ../cpan/version/lib\' ;',
            'start_line' => 5,
            'end_line' => 5,
            'has_warnings' => 0,
            'token_num' => 5,
            'block_id' => 1
          },
          {
            'block_id' => 1,
            'token_num' => 3,
            'end_line' => 6,
            'has_warnings' => 0,
            'src' => ' require \'./test.pl\' ;',
            'indent' => 1,
            'start_line' => 6
          },
          {
            'end_line' => 16,
            'has_warnings' => 0,
            'token_num' => 12,
            'block_id' => 0,
            'src' => ' my @syntax_cases = ( \'package Foo\' , \'package Bar 1.23\' , \'package Baz v1.2.3\' , ) ;',
            'indent' => 0,
            'start_line' => 12
          },
          {
            'start_line' => 18,
            'indent' => 0,
            'src' => ' my @version_cases = < DATA > ;',
            'block_id' => 0,
            'token_num' => 7,
            'end_line' => 18,
            'has_warnings' => 1
          },
          {
            'indent' => 0,
            'src' => ' plan tests => 7 * @syntax_cases + 7 * ( grep { $_ !~/^#/ } @version_cases ) + 2 * 3 ;',
            'start_line' => 20,
            'has_warnings' => 1,
            'end_line' => 21,
            'token_num' => 25,
            'block_id' => 0
          },
          {
            'has_warnings' => 0,
            'end_line' => 23,
            'block_id' => 0,
            'token_num' => 7,
            'start_line' => 23,
            'indent' => 0,
            'src' => ' use warnings qw/syntax/ ;'
          },
          {
            'end_line' => 24,
            'has_warnings' => 0,
            'token_num' => 3,
            'block_id' => 0,
            'indent' => 0,
            'src' => ' use version ;',
            'start_line' => 24
          },
          {
            'has_warnings' => 1,
            'end_line' => 41,
            'token_num' => 113,
            'block_id' => 0,
            'indent' => 0,
            'src' => ' for my $string ( @syntax_cases ) { eval "$string" ; is ( $@ , \'\' , qq/eval "$string"/ ) ; eval "$string;" ; is ( $@ , \'\' , qq/eval "$string;"/ ) ; eval "$string ;" ; is ( $@ , \'\' , qq/eval "$string ;"/ ) ; eval "{$string}" ; is ( $@ , \'\' , qq/eval "{$string}"/ ) ; eval "{ $string }" ; is ( $@ , \'\' , qq/eval "{ $string }"/ ) ; eval "${string}{}" ; is ( $@ , \'\' , qq/eval "${string}{}"/ ) ; eval "$string {}" ; is ( $@ , \'\' , qq/eval "$string {}"/ ) ; }',
            'start_line' => 26
          },
          {
            'src' => ' eval "$string" ;',
            'indent' => 1,
            'start_line' => 27,
            'block_id' => 2,
            'token_num' => 3,
            'end_line' => 27,
            'has_warnings' => 0
          },
