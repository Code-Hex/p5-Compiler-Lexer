            'src' => ' { my $x ; sub f { "abc" =~/(.)./ ; $x = "@+" ; return @+ ; } ; "pqrstuvwxyz" =~/..(....)../ ; my @y = f ( ) ; is $x , "@y" , "return a magic array ($x) vs (@y)" ; sub f2 { "abc" =~/(?<foo>.)./ ; my @h = %+ ; $x = "@h" ; return %+ ; } ; @y = f ( ) ; is $x , "@y" , "return a magic hash ($x) vs (@y)" ; }'
