            'block_id' => 1,
            'token_num' => 6,
            'has_warnings' => 0,
            'end_line' => 5
          },
          {
            'end_line' => 6,
            'has_warnings' => 0,
            'token_num' => 3,
            'block_id' => 1,
            'src' => ' require \'./test.pl\' ;',
            'indent' => 1,
            'start_line' => 6
          },
          {
            'src' => ' skip_all_if_miniperl ( "no dynamic loading on miniperl, no Tie::Hash::NamedCapture" ) ;',
