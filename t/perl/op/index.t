subtest 'get_used_modules' => sub {
    my $modules = Compiler::Lexer->new('')->get_used_modules($script);
    is_deeply($modules, [
          {
            'args' => '',
            'name' => 'strict'
          },
          {
            'name' => 'constant',
            'args' => '  { PVBM => \'galumphing\' , PVBM2 => \'bang\' }'
          },
          {
            'args' => '  perl  =>  "rules"',
            'name' => 'constant'
          },
          {
            'args' => '  riffraff  =>  \\  our  $referent',
            'name' => 'constant'
          },
          {
            'args' => '  \'""\'  =>  sub  { "foo" }  }',
            'name' => 'overload'
          },
          {
            'args' => '  quire  =>  ${ qr/(?{})/ }',
            'name' => 'constant'
          },
          {
            'args' => '  bang  =>  $!  =  8',
            'name' => 'constant'
          },
          {
            'name' => 'constant',
            'args' => '  u  =>  undef'
          }
        ]
, 'Compiler::Lexer::get_used_modules');
};

