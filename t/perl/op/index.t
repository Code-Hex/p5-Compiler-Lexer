subtest 'get_groups_by_syntax_level' => sub {
    my $lexer = Compiler::Lexer->new('');
    my $tokens = $lexer->tokenize($script);
    my $stmts = $lexer->get_groups_by_syntax_level($tokens, Compiler::Lexer::SyntaxType::T_Stmt);
    is_deeply($stmts, [
          {
            'has_warnings' => 0,
            'end_line' => 4,
            'token_num' => 6,
            'src' => ' chdir \'t\' if -d \'t\' ;',
            'start_line' => 4,
            'block_id' => 1,
            'indent' => 1
          },
          {
            'end_line' => 5,
            'has_warnings' => 0,
            'indent' => 1,
            'start_line' => 5,
            'block_id' => 1,
            'token_num' => 3,
            'src' => ' require \'./test.pl\' ;'
          },
          {
            'src' => ' set_up_inc ( \'../lib\' ) ;',
            'token_num' => 5,
            'block_id' => 1,
            'start_line' => 6,
            'indent' => 1,
            'has_warnings' => 1,
            'end_line' => 6
          },
          {
            'end_line' => 7,
            'has_warnings' => 0,
            'indent' => 1,
            'start_line' => 7,
            'block_id' => 1,
            'token_num' => 3,
            'src' => ' require \'./charset_tools.pl\' ;'
          },
          {
            'token_num' => 3,
            'src' => ' use strict ;',
            'start_line' => 10,
            'indent' => 0,
            'block_id' => 0,
            'has_warnings' => 0,
            'end_line' => 10
          },
          {
            'end_line' => 11,
            'has_warnings' => 1,
            'start_line' => 11,
            'block_id' => 0,
            'indent' => 0,
            'token_num' => 7,
            'src' => ' plan ( tests => 122 ) ;'
          },
          {
            'has_warnings' => 1,
            'end_line' => 13,
            'src' => ' run_tests ( ) unless caller ;',
            'token_num' => 6,
            'start_line' => 13,
            'indent' => 0,
            'block_id' => 0
          },
          {
            'end_line' => 258,
            'has_warnings' => 1,
            'start_line' => 15,
