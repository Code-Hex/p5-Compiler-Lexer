subtest 'get_groups_by_syntax_level' => sub {
    my $lexer = Compiler::Lexer->new('');
    my $tokens = $lexer->tokenize($script);
    my $stmts = $lexer->get_groups_by_syntax_level($tokens, Compiler::Lexer::SyntaxType::T_Stmt);
    is_deeply($stmts, [
          {
            'has_warnings' => 0,
            'end_line' => 4,
            'src' => ' chdir \'t\' if -d \'t\' ;',
            'token_num' => 6,
            'indent' => 1,
            'start_line' => 4,
            'block_id' => 1
          },
          {
            'has_warnings' => 0,
            'end_line' => 5,
            'token_num' => 7,
            'src' => ' @INC = qw(. ../lib) ;',
            'indent' => 1,
            'start_line' => 5,
            'block_id' => 1
          },
          {
            'has_warnings' => 0,
            'end_line' => 8,
            'src' => ' use strict ;',
            'token_num' => 3,
            'start_line' => 8,
            'indent' => 0,
            'block_id' => 0
          },
          {
            'start_line' => 9,
            'block_id' => 0,
            'indent' => 0,
            'src' => ' use warnings ;',
            'token_num' => 3,
            'end_line' => 9,
            'has_warnings' => 0
          },
          {
            'token_num' => 3,
            'src' => ' require \'./test.pl\' ;',
            'start_line' => 12,
            'indent' => 1,
            'block_id' => 2,
            'has_warnings' => 0,
            'end_line' => 12
          },
          {
            'src' => ' plan ( tests => 12 ) ;',
            'token_num' => 7,
            'indent' => 1,
            'start_line' => 13,
            'block_id' => 2,
            'has_warnings' => 1,
            'end_line' => 13
          },
          {
            'token_num' => 7,
            'src' => ' use vars qw{ @warnings $sub $warn } ;',
            'start_line' => 16,
            'block_id' => 0,
            'indent' => 0,
            'has_warnings' => 0,
            'end_line' => 16
          },
          {
            'token_num' => 4,
            'src' => ' $warn = \'Illegal character in prototype\' ;',
            'indent' => 1,
            'start_line' => 19,
            'block_id' => 3,
            'has_warnings' => 1,
            'end_line' => 19
          },
          {
            'end_line' => 26,
            'has_warnings' => 1,
            'start_line' => 22,
            'block_id' => 0,
            'indent' => 0,
            'src' => ' sub one_warning_ok { cmp_ok ( scalar ( @warnings ) , \'==\' , 1 , \'One warning\' ) ; cmp_ok ( substr ( $warnings [ 0 ] , 0 , length ( $warn ) ) , \'eq\' , $warn , \'warning message\' ) ; @warnings = ( ) ; }',
            'token_num' => 47
          },
          {
            'src' => ' cmp_ok ( scalar ( @warnings ) , \'==\' , 1 , \'One warning\' ) ;',
            'token_num' => 14,
            'block_id' => 4,
            'start_line' => 23,
            'indent' => 1,
            'has_warnings' => 1,
            'end_line' => 23
          },
          {
            'end_line' => 24,
            'has_warnings' => 1,
            'block_id' => 4,
            'start_line' => 24,
            'indent' => 1,
            'src' => ' cmp_ok ( substr ( $warnings [ 0 ] , 0 , length ( $warn ) ) , \'eq\' , $warn , \'warning message\' ) ;',
            'token_num' => 24
          },
          {
            'indent' => 1,
            'start_line' => 25,
            'block_id' => 4,
            'src' => ' @warnings = ( ) ;',
            'token_num' => 5,
            'end_line' => 25,
            'has_warnings' => 0
          },
          {
            'end_line' => 31,
            'has_warnings' => 1,
            'block_id' => 0,
            'start_line' => 28,
            'indent' => 0,
            'token_num' => 23,
            'src' => ' sub no_warnings_ok { cmp_ok ( scalar ( @warnings ) , \'==\' , 0 , \'No warnings\' ) ; @warnings = ( ) ; }'
          },
          {
            'end_line' => 29,
            'has_warnings' => 1,
            'block_id' => 5,
            'start_line' => 29,
            'indent' => 1,
            'token_num' => 14,
            'src' => ' cmp_ok ( scalar ( @warnings ) , \'==\' , 0 , \'No warnings\' ) ;'
          },
          {
            'end_line' => 30,
            'has_warnings' => 0,
            'start_line' => 30,
            'indent' => 1,
            'block_id' => 5,
            'src' => ' @warnings = ( ) ;',
            'token_num' => 5
          },
          {
            'block_id' => 6,
            'start_line' => 34,
            'indent' => 1,
            'token_num' => 13,
            'src' => ' $SIG { \'__WARN__\' } = sub { push @warnings , @_ } ;',
            'end_line' => 34,
            'has_warnings' => 1
          },
          {
            'src' => ' $| = 1 ;',
            'token_num' => 4,
            'indent' => 1,
            'start_line' => 35,
            'block_id' => 6,
            'has_warnings' => 0,
            'end_line' => 35
          }
        ]
