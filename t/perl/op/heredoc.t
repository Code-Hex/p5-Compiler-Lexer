subtest 'get_groups_by_syntax_level' => sub {
    my $lexer = Compiler::Lexer->new('');
    my $tokens = $lexer->tokenize($script);
    my $stmts = $lexer->get_groups_by_syntax_level($tokens, Compiler::Lexer::SyntaxType::T_Stmt);
    is_deeply($stmts, [
          {
            'indent' => 1,
            'start_line' => 4,
            'block_id' => 1,
            'token_num' => 6,
            'src' => ' chdir \'t\' if -d \'t\' ;',
            'end_line' => 4,
            'has_warnings' => 0
          },
          {
            'end_line' => 5,
            'has_warnings' => 0,
            'indent' => 1,
            'start_line' => 5,
            'block_id' => 1,
            'src' => ' @INC = \'../lib\' ;',
            'token_num' => 4
          },
          {
            'token_num' => 3,
            'src' => ' require \'./test.pl\' ;',
            'indent' => 1,
            'start_line' => 6,
            'block_id' => 1,
            'has_warnings' => 0,
            'end_line' => 6
          },
          {
            'end_line' => 9,
            'has_warnings' => 0,
            'start_line' => 9,
            'indent' => 0,
            'block_id' => 0,
            'token_num' => 3,
            'src' => ' use strict ;'
          },
          {
            'has_warnings' => 1,
            'end_line' => 10,
            'token_num' => 7,
            'src' => ' plan ( tests => 41 ) ;',
            'start_line' => 10,
            'block_id' => 0,
            'indent' => 0
          },
          {
            'start_line' => 14,
            'block_id' => 0,
            'indent' => 0,
            'src' => ' { my $string = q{testing for 65838
