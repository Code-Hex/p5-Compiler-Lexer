subtest 'get_groups_by_syntax_level' => sub {
    my $lexer = Compiler::Lexer->new('');
    my $tokens = $lexer->tokenize($script);
    my $stmts = $lexer->get_groups_by_syntax_level($tokens, Compiler::Lexer::SyntaxType::T_Stmt);
    is_deeply($stmts, [
          {
            'indent' => 1,
            'src' => ' chdir \'t\' if -d \'t\' ;',
            'start_line' => 8,
            'token_num' => 6,
            'block_id' => 1,
            'end_line' => 8,
            'has_warnings' => 0
          },
          {
            'block_id' => 1,
            'token_num' => 4,
            'end_line' => 9,
            'has_warnings' => 0,
            'indent' => 1,
            'src' => ' @INC = \'../lib\' ;',
            'start_line' => 9
          },
          {
            'end_line' => 10,
            'has_warnings' => 0,
            'token_num' => 3,
            'block_id' => 1,
            'start_line' => 10,
            'src' => ' require \'./test.pl\' ;',
            'indent' => 1
          },
          {
            'end_line' => 13,
            'has_warnings' => 0,
            'token_num' => 8,
            'block_id' => 0,
            'start_line' => 13,
            'indent' => 0,
            'src' => ' my @ops = split // , \'rwxoRWXOezsfdlpSbctugkTMBAC\' ;'
          },
          {
            'token_num' => 9,
            'block_id' => 0,
            'end_line' => 15,
            'has_warnings' => 1,
            'start_line' => 15,
            'indent' => 0,
            'src' => ' plan ( tests => @ops * 5 ) ;'
          },
          {
            'src' => ' sub { 1 }',
            'indent' => 1,
            'start_line' => 17,
            'end_line' => 17,
            'has_warnings' => 0,
            'token_num' => 4,
            'block_id' => 2
          },
          {
            'start_line' => 18,
            'src' => ' my $o = bless [ ] , \'o\' ;',
            'indent' => 0,
            'block_id' => 0,
            'token_num' => 9,
            'end_line' => 18,
            'has_warnings' => 0
          },
          {
            'src' => ' for my $op ( @ops ) { ok ( 1 == @{ [ eval "-$op \'TEST\'" ] } , "-$op returns single value" ) ;',
            'indent' => 0,
            'start_line' => 20,
            'block_id' => 0,
            'token_num' => 21,
            'has_warnings' => 1,
            'end_line' => 21
          },
          {
            'start_line' => 20,
            'src' => ' for my $op ( @ops ) { ok ( 1 == @{ [ eval "-$op \'TEST\'" ] } , "-$op returns single value" )',
            'indent' => 0,
            'token_num' => 20,
            'block_id' => 3,
            'has_warnings' => 1,
            'end_line' => 21
          }
        ]
, 'Compiler::Lexer::get_groups_by_syntax_level');
};

subtest 'get_used_modules' => sub {
    my $modules = Compiler::Lexer->new('')->get_used_modules($script);
    is_deeply($modules, [
          {
            'args' => '  \'-X\'  =>  sub  { 1 }  }',
            'name' => 'overload'
          }
        ]
, 'Compiler::Lexer::get_used_modules');
};

