          },
          {
            'start_line' => 34,
            'indent' => 0,
            'src' => ' ok ! -e \'zzz\' , \'unlink with no arg unlinked $_\' ;',
            'has_warnings' => 1,
            'end_line' => 34,
            'token_num' => 7,
            'block_id' => 0
          },
          {
            'token_num' => 6,
            'block_id' => 0,
            'has_warnings' => 0,
            'end_line' => 37,
            'indent' => 0,
            'src' => ' chdir \'..\' or die "Couldn\'t chdir .. for cleanup: $!" ;',
            'start_line' => 37
          },
          {
            'src' => ' rmdir $tempdir or die "Couldn\'t unlink tempdir \'$tempdir\': $!" ;',
