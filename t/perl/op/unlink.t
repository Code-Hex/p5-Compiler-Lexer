subtest 'get_groups_by_syntax_level' => sub {
    my $lexer = Compiler::Lexer->new('');
    my $tokens = $lexer->tokenize($script);
    my $stmts = $lexer->get_groups_by_syntax_level($tokens, Compiler::Lexer::SyntaxType::T_Stmt);
    is_deeply($stmts, [
          {
            'start_line' => 4,
            'src' => ' chdir \'t\' if -d \'t\' ;',
            'indent' => 1,
            'block_id' => 1,
            'token_num' => 6,
            'end_line' => 4,
            'has_warnings' => 0
          },
          {
            'block_id' => 1,
            'token_num' => 4,
            'has_warnings' => 0,
            'end_line' => 5,
            'start_line' => 5,
            'src' => ' @INC = \'../lib\' ;',
            'indent' => 1
          },
          {
            'block_id' => 1,
            'token_num' => 3,
            'has_warnings' => 0,
            'end_line' => 6,
            'indent' => 1,
            'src' => ' require \'./test.pl\' ;',
            'start_line' => 6
          },
          {
            'has_warnings' => 1,
            'end_line' => 9,
            'block_id' => 0,
            'token_num' => 3,
            'src' => ' plan 6 ;',
            'indent' => 0,
            'start_line' => 9
          },
          {
            'indent' => 0,
            'src' => ' my $tempdir = tempfile ;',
            'start_line' => 13,
            'end_line' => 13,
            'has_warnings' => 1,
            'token_num' => 5,
            'block_id' => 0
          },
          {
            'block_id' => 0,
            'token_num' => 8,
            'has_warnings' => 1,
            'end_line' => 15,
            'start_line' => 15,
            'indent' => 0,
            'src' => ' mkdir $tempdir , 0700 or die "Can\'t mkdir \'$tempdir\': $!" ;'
          },
          {
            'src' => ' chdir $tempdir or die die "Can\'t chdir \'$tempdir\': $!" ;',
            'indent' => 0,
            'start_line' => 16,
            'has_warnings' => 1,
            'end_line' => 16,
            'token_num' => 7,
            'block_id' => 0
          },
          {
            'indent' => 0,
