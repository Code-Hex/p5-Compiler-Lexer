            'block_id' => 2,
            'token_num' => 11,
            'indent' => 1,
            'src' => ' open my $fh , ">" , $file or die "Can\'t open $file: $!" ;',
            'start_line' => 20
          },
          {
            'start_line' => 21,
            'indent' => 1,
            'src' => ' close $fh or die "Can\'t close $file: $!" ;',
            'has_warnings' => 1,
            'end_line' => 21,
            'token_num' => 6,
