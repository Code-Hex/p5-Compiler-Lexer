        *File::Glob::csh_glob = sub { ++$called2 };
        my $output2 = eval q{ glob(q(./"TEST")) };
        print "ok1" if $called1 eq $called2;
        print "ok2" if $output1 eq $output2;
    }
} , $expected , { switches => $switches } , $name ) ;',
            'indent' => 1,
            'start_line' => 125
          }
        ]
, 'Compiler::Lexer::get_groups_by_syntax_level');
};

