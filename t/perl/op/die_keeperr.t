subtest 'get_used_modules' => sub {
    my $modules = Compiler::Lexer->new('')->get_used_modules($script);
    is_deeply($modules, [
          {
            'args' => '  "misc"',
            'name' => 'warnings'
          },
          {
            'name' => 'warnings',
            'args' => '  "misc"'
          },
          {
            'name' => 'warnings',
            'args' => '  "misc"'
          },
          {
            'args' => '  "misc"',
            'name' => 'warnings'
          },
          {
            'name' => 'warnings',
            'args' => '  "misc"'
          }
        ]
, 'Compiler::Lexer::get_used_modules');
};

done_testing;
