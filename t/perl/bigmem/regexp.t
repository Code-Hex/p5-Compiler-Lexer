subtest 'get_groups_by_syntax_level' => sub {
    my $lexer = Compiler::Lexer->new('');
    my $tokens = $lexer->tokenize($script);
    my $stmts = $lexer->get_groups_by_syntax_level($tokens, Compiler::Lexer::SyntaxType::T_Stmt);
    is_deeply($stmts, [
          {
            'src' => ' chdir \'t\' if -d \'t\' ;',
            'indent' => 1,
            'start_line' => 3,
            'end_line' => 3,
            'has_warnings' => 0,
            'block_id' => 1,
            'token_num' => 6
          },
          {
            'end_line' => 4,
            'has_warnings' => 0,
            'token_num' => 4,
            'block_id' => 1,
            'src' => ' @INC = "../lib" ;',
            'indent' => 1,
            'start_line' => 4
          },
          {
            'indent' => 1,
            'src' => ' require \'./test.pl\' ;',
            'start_line' => 5,
            'end_line' => 5,
            'has_warnings' => 0,
            'token_num' => 3,
            'block_id' => 1
          },
          {
            'token_num' => 7,
            'block_id' => 0,
            'has_warnings' => 0,
            'end_line' => 8,
            'indent' => 0,
            'src' => ' use Config qw(%Config) ;',
            'start_line' => 8
          },
          {
            'start_line' => 10,
            'src' => ' $ENV { PERL_TEST_MEMORY } >= 2 or skip_all ( "Need ~2Gb for this test" ) ;',
            'indent' => 0,
            'has_warnings' => 1,
            'end_line' => 11,
            'block_id' => 0,
            'token_num' => 12
          },
          {
            'block_id' => 0,
            'token_num' => 12,
            'end_line' => 13,
            'has_warnings' => 1,
            'start_line' => 12,
            'indent' => 0,
            'src' => ' $Config { ptrsize } >= 8 or skip_all ( "Need 64-bit pointers for this test" ) ;'
          },
          {
            'has_warnings' => 1,
            'end_line' => 15,
            'block_id' => 0,
            'token_num' => 5,
            'src' => ' plan ( 6 ) ;',
            'indent' => 0,
            'start_line' => 15
          },
          {
            'start_line' => 20,
            'indent' => 0,
            'src' => ' my $x = " " x ( ${ \\ 2 } ** 31 ) . "abcdefg" ;',
            'token_num' => 16,
            'block_id' => 0,
            'has_warnings' => 0,
            'end_line' => 20
          },
          {
            'src' => ' ok $x =~/./ , \'match against long string succeeded\' ;',
            'indent' => 0,
            'start_line' => 21,
            'has_warnings' => 1,
            'end_line' => 21,
            'token_num' => 9,
            'block_id' => 0
          },
          {
            'token_num' => 7,
            'block_id' => 0,
            'has_warnings' => 1,
            'end_line' => 22,
            'src' => ' is "$-[0]-$+[0]" , \'0-1\' , \'@-/@+ after match against long string\' ;',
            'indent' => 0,
            'start_line' => 22
          },
          {
            'indent' => 0,
            'src' => ' pos $x = 2 ** 31 -1 ;',
            'start_line' => 24,
            'token_num' => 8,
            'block_id' => 0,
            'has_warnings' => 1,
            'end_line' => 24
          },
          {
            'end_line' => 25,
            'has_warnings' => 0,
            'block_id' => 0,
            'token_num' => 3,
            'start_line' => 25,
            'src' => ' my $result ;',
            'indent' => 0
          },
          {
            'start_line' => 26,
            'src' => ' for ( 1 .. 5 ) { $x =~/./g ; $result .= "$&-" ; }',
            'indent' => 0,
            'has_warnings' => 1,
            'end_line' => 29,
            'token_num' => 19,
            'block_id' => 0
          },
          {
            'block_id' => 2,
            'token_num' => 7,
            'has_warnings' => 1,
            'end_line' => 27,
            'indent' => 1,
            'src' => ' $x =~/./g ;',
            'start_line' => 27
          },
          {
            'src' => ' $result .= "$&-" ;',
            'indent' => 1,
            'start_line' => 28,
            'block_id' => 2,
            'token_num' => 4,
            'end_line' => 28,
            'has_warnings' => 1
          },
          {
            'start_line' => 30,
            'indent' => 0,
            'src' => ' is $result , " -a-b-c-d-" , \'scalar //g hopping past the 2**31 threshold\' ;',
            'end_line' => 30,
            'has_warnings' => 1,
            'token_num' => 7,
            'block_id' => 0
          },
          {
            'end_line' => 31,
            'has_warnings' => 1,
            'block_id' => 0,
            'token_num' => 9,
            'start_line' => 31,
            'src' => ' pos $x = 2 ** 31 + 3 ;',
            'indent' => 0
          },
          {
            'src' => ' $x =~/./g ;',
            'indent' => 0,
            'start_line' => 32,
            'token_num' => 7,
            'block_id' => 0,
            'has_warnings' => 1,
            'end_line' => 32
          },
          {
            'end_line' => 33,
            'has_warnings' => 1,
            'token_num' => 8,
            'block_id' => 0,
            'start_line' => 33,
            'indent' => 0,
            'src' => ' is "$\'" , \'efg\' , q "$\' after match against long string" ;'
          },
          {
            'start_line' => 34,
            'src' => ' is "$-[0],$+[0]" , \'2147483651,2147483652\' , \'@- and @+ after matches past 2**31\' ;',
            'indent' => 0,
            'has_warnings' => 1,
            'end_line' => 35,
            'token_num' => 7,
            'block_id' => 0
          },
          {
            'indent' => 0,
            'src' => ' is $x =~/(?:(?:.{32766}){32766}){2}(?:.{32766}){8}.{8}ef/ , 1 , \'anchored substr past 2**31\' ;',
            'start_line' => 38,
            'end_line' => 39,
            'has_warnings' => 1,
            'token_num' => 11,
            'block_id' => 0
          }
        ]
, 'Compiler::Lexer::get_groups_by_syntax_level');
};

subtest 'get_used_modules' => sub {
    my $modules = Compiler::Lexer->new('')->get_used_modules($script);
