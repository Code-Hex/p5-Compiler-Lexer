subtest 'get_used_modules' => sub {
    my $modules = Compiler::Lexer->new('')->get_used_modules($script);
    is_deeply($modules, [
          {
            'name' => 'bytes',
            'args' => ''
          },
          {
            'args' => '',
            'name' => 'bytes'
          },
          {
            'name' => 'bytes',
            'args' => ''
          },
          {
            'args' => '',
            'name' => 'bytes'
          },
          {
            'args' => '  \'utf8\'',
            'name' => 'warnings'
          },
          {
            'name' => 'warnings',
            'args' => '  \'utf8\''
          },
          {
            'args' => '  \'utf8\'',
            'name' => 'warnings'
          },
          {
            'args' => '  \'utf8\'',
            'name' => 'warnings'
          }
        ]
, 'Compiler::Lexer::get_used_modules');
};

done_testing;
