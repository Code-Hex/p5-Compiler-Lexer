subtest 'get_groups_by_syntax_level' => sub {
    my $lexer = Compiler::Lexer->new('');
    my $tokens = $lexer->tokenize($script);
    my $stmts = $lexer->get_groups_by_syntax_level($tokens, Compiler::Lexer::SyntaxType::T_Stmt);
    is_deeply($stmts, [
          {
            'end_line' => 2,
            'has_warnings' => 0,
            'block_id' => 1,
            'start_line' => 2,
            'indent' => 1,
            'src' => ' chdir \'t\' if -d \'t\' ;',
            'token_num' => 6
          },
          {
            'has_warnings' => 0,
            'end_line' => 3,
            'src' => ' @INC = \'../lib\' if -d \'../lib\' && -d \'../ext\' ;',
            'token_num' => 10,
            'block_id' => 1,
            'start_line' => 3,
            'indent' => 1
          },
          {
            'has_warnings' => 0,
            'end_line' => 5,
            'src' => ' require "./test.pl" ;',
            'token_num' => 3,
            'block_id' => 1,
            'start_line' => 5,
            'indent' => 1
          },
          {
            'src' => ' require Config ;',
            'token_num' => 3,
            'block_id' => 1,
            'start_line' => 6,
            'indent' => 1,
            'has_warnings' => 0,
            'end_line' => 6
          },
          {
            'indent' => 1,
            'start_line' => 6,
            'block_id' => 1,
            'token_num' => 3,
            'src' => ' import Config ;',
            'end_line' => 6,
            'has_warnings' => 1
          },
          {
            'end_line' => 9,
            'has_warnings' => 0,
            'start_line' => 9,
            'block_id' => 0,
            'indent' => 0,
            'src' => ' use strict ;',
            'token_num' => 3
          },
          {
            'src' => ' use warnings ;',
            'token_num' => 3,
            'start_line' => 10,
            'block_id' => 0,
            'indent' => 0,
            'has_warnings' => 0,
            'end_line' => 10
          },
          {
            'start_line' => 12,
            'block_id' => 0,
            'indent' => 0,
            'token_num' => 11,
            'src' => ' skip_all ( \'no SysV semaphores on this platform\' ) if ! $Config { d_sem } ;',
            'end_line' => 12,
            'has_warnings' => 1
          },
          {
            'start_line' => 13,
            'block_id' => 0,
            'indent' => 0,
            'src' => ' skip_all ( \'SysV semaphore structures not detected\' ) if ! $Config { d_semctl_semid_ds } && ! $Config { d_semctl_semun } ;',
            'token_num' => 17,
            'end_line' => 14,
            'has_warnings' => 1
          },
          {
            'has_warnings' => 0,
            'end_line' => 16,
            'src' => ' my @warnings ;',
            'token_num' => 3,
            'block_id' => 0,
            'start_line' => 16,
            'indent' => 0
          },
          {
            'indent' => 0,
            'start_line' => 17,
            'block_id' => 0,
            'token_num' => 67,
            'src' => ' { local $SIG { __WARN__ } = sub { push @warnings , $_ [ 0 ] } ; local $SIG { SYS } = sub { skip_all ( "SIGSYS caught" ) } if exists $SIG { SYS } ; my $test = ( semctl ( -1 , 0 , 0 , 0 ) ) [ 0 ] ; ok ( ! defined $test , "erroneous semctl list slice yields undef" ) ; }',
            'end_line' => 22,
            'has_warnings' => 1
          },
          {
            'has_warnings' => 1,
            'end_line' => 18,
            'token_num' => 17,
            'src' => ' local $SIG { __WARN__ } = sub { push @warnings , $_ [ 0 ] } ;',
            'block_id' => 2,
            'start_line' => 18,
            'indent' => 1
          },
          {
            'token_num' => 20,
            'src' => ' local $SIG { SYS } = sub { skip_all ( "SIGSYS caught" ) } if exists $SIG { SYS } ;',
            'start_line' => 19,
            'block_id' => 2,
            'indent' => 1,
            'has_warnings' => 1,
            'end_line' => 19
          },
          {
            'block_id' => 2,
            'start_line' => 20,
            'indent' => 1,
            'token_num' => 19,
            'src' => ' my $test = ( semctl ( -1 , 0 , 0 , 0 ) ) [ 0 ] ;',
            'end_line' => 20,
            'has_warnings' => 0
          },
          {
            'end_line' => 21,
            'has_warnings' => 1,
            'block_id' => 2,
            'start_line' => 21,
            'indent' => 1,
            'token_num' => 9,
            'src' => ' ok ( ! defined $test , "erroneous semctl list slice yields undef" ) ;'
          },
          {
            'end_line' => 25,
            'has_warnings' => 1,
            'start_line' => 24,
            'indent' => 0,
            'block_id' => 0,
            'src' => ' is ( scalar @warnings , 0 , "no warnings from erroneous semctl list slice" ) or diag ( "warnings found: @warnings" ) ;',
            'token_num' => 15
          },
          {
            'end_line' => 27,
            'has_warnings' => 1,
            'block_id' => 0,
            'start_line' => 27,
            'indent' => 0,
            'src' => ' done_testing ;',
            'token_num' => 2
          }
        ]
, 'Compiler::Lexer::get_groups_by_syntax_level');
};

