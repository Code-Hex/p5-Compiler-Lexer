subtest 'get_groups_by_syntax_level' => sub {
    my $lexer = Compiler::Lexer->new('');
    my $tokens = $lexer->tokenize($script);
    my $stmts = $lexer->get_groups_by_syntax_level($tokens, Compiler::Lexer::SyntaxType::T_Stmt);
    is_deeply($stmts, [
          {
            'src' => ' chdir \'t\' if -d \'t\' ;',
            'indent' => 1,
            'start_line' => 8,
            'has_warnings' => 0,
            'end_line' => 8,
            'block_id' => 1,
            'token_num' => 6
          },
          {
            'end_line' => 9,
            'has_warnings' => 0,
            'block_id' => 1,
            'token_num' => 4,
            'start_line' => 9,
            'src' => ' @INC = \'../lib\' ;',
            'indent' => 1
          },
          {
            'block_id' => 1,
