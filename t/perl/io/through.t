subtest 'get_groups_by_syntax_level' => sub {
    my $lexer = Compiler::Lexer->new('');
    my $tokens = $lexer->tokenize($script);
    my $stmts = $lexer->get_groups_by_syntax_level($tokens, Compiler::Lexer::SyntaxType::T_Stmt);
    is_deeply($stmts, [
          {
            'token_num' => 6,
            'src' => ' chdir \'t\' if -d \'t\' ;',
            'start_line' => 4,
            'indent' => 1,
            'block_id' => 1,
            'has_warnings' => 0,
            'end_line' => 4
          },
          {
            'has_warnings' => 0,
            'end_line' => 5,
            'token_num' => 4,
            'src' => ' @INC = \'../lib\' ;',
            'indent' => 1,
            'start_line' => 5,
            'block_id' => 1
          },
          {
            'start_line' => 6,
            'indent' => 1,
            'block_id' => 1,
            'src' => ' require \'./test.pl\' ;',
            'token_num' => 3,
            'end_line' => 6,
            'has_warnings' => 0
          },
          {
            'end_line' => 8,
            'has_warnings' => 1,
            'start_line' => 7,
            'block_id' => 1,
            'indent' => 1,
            'token_num' => 9,
            'src' => ' skip_all ( "VMS too picky about line endings for record-oriented pipes" ) if $^O eq \'VMS\' ;'
          },
          {
            'has_warnings' => 0,
            'end_line' => 11,
            'token_num' => 3,
            'src' => ' use strict ;',
            'start_line' => 11,
            'indent' => 0,
            'block_id' => 0
          },
          {
            'end_line' => 13,
            'has_warnings' => 1,
            'start_line' => 13,
            'indent' => 0,
            'block_id' => 0,
            'token_num' => 7,
            'src' => ' my $Perl = which_perl ( ) ;'
          },
          {
