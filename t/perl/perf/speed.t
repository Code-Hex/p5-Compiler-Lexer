          },
          {
            'name' => 'warnings',
            'args' => ''
          },
          {
            'args' => '',
            'name' => '5.010'
          },
          {
            'args' => '',
            'name' => 'warnings'
          },
          {
            'args' => '',
            'name' => 'strict'
          }
        ]
, 'Compiler::Lexer::get_used_modules');
};

