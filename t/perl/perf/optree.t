subtest 'get_groups_by_syntax_level' => sub {
    my $lexer = Compiler::Lexer->new('');
    my $tokens = $lexer->tokenize($script);
    my $stmts = $lexer->get_groups_by_syntax_level($tokens, Compiler::Lexer::SyntaxType::T_Stmt);
    is_deeply($stmts, [
          {
            'src' => ' chdir \'t\' ;',
            'token_num' => 3,
            'block_id' => 1,
            'start_line' => 7,
            'indent' => 1,
            'has_warnings' => 0,
            'end_line' => 7
          },
          {
            'token_num' => 3,
            'src' => ' require \'./test.pl\' ;',
            'block_id' => 1,
            'start_line' => 8,
            'indent' => 1,
            'has_warnings' => 0,
            'end_line' => 8
          },
          {
            'start_line' => 9,
            'block_id' => 1,
            'indent' => 1,
            'token_num' => 5,
            'src' => ' skip_all_if_miniperl ( "No B under miniperl" ) ;',
            'end_line' => 9,
            'has_warnings' => 1
          },
          {
            'token_num' => 4,
            'src' => ' @INC = \'../lib\' ;',
            'indent' => 1,
            'start_line' => 10,
            'block_id' => 1,
            'has_warnings' => 0,
            'end_line' => 10
          },
          {
            'src' => ' plan 54 ;',
            'token_num' => 3,
            'start_line' => 13,
            'block_id' => 0,
            'indent' => 0,
            'has_warnings' => 1,
            'end_line' => 13
          },
          {
