subtest 'get_groups_by_syntax_level' => sub {
    my $lexer = Compiler::Lexer->new('');
    my $tokens = $lexer->tokenize($script);
    my $stmts = $lexer->get_groups_by_syntax_level($tokens, Compiler::Lexer::SyntaxType::T_Stmt);
    is_deeply($stmts, [
          {
            'block_id' => 0,
            'token_num' => 3,
            'end_line' => 2,
            'has_warnings' => 0,
            'src' => ' use strict ;',
            'indent' => 0,
            'start_line' => 2
          },
          {
            'end_line' => 3,
            'has_warnings' => 0,
            'token_num' => 3,
            'block_id' => 0,
            'start_line' => 3,
            'src' => ' use warnings ;',
            'indent' => 0
          },
          {
            'token_num' => 4,
            'block_id' => 0,
            'end_line' => 5,
            'has_warnings' => 0,
            'indent' => 0,
            'src' => ' $| = 1 ;',
            'start_line' => 5
          },
          {
            'end_line' => 9,
            'has_warnings' => 0,
            'block_id' => 1,
            'token_num' => 6,
            'start_line' => 9,
            'indent' => 1,
            'src' => ' chdir \'t\' if -d \'t\' ;'
          },
          {
            'end_line' => 10,
            'has_warnings' => 0,
            'token_num' => 10,
            'block_id' => 1,
            'src' => ' @INC = ( \'../lib\' , \'.\' , \'../ext/re\' ) ;',
            'indent' => 1,
            'start_line' => 10
          },
          {
            'start_line' => 11,
            'src' => ' require \'./test.pl\' ;',
            'indent' => 1,
            'has_warnings' => 0,
            'end_line' => 11,
            'block_id' => 1,
            'token_num' => 3
          },
          {
            'has_warnings' => 1,
            'end_line' => 15,
            'token_num' => 18,
            'block_id' => 1,
            'src' => ' if ( is_miniperl ( ) ) { skip_all_if_miniperl ( "Unicode tables not built yet" , 2 ) unless eval \'require "unicore/Heavy.pl"\' ; }',
            'indent' => 1,
            'start_line' => 12
          },
          {
            'has_warnings' => 1,
            'end_line' => 14,
            'token_num' => 10,
            'block_id' => 2,
            'start_line' => 13,
            'indent' => 2,
            'src' => ' skip_all_if_miniperl ( "Unicode tables not built yet" , 2 ) unless eval \'require "unicore/Heavy.pl"\' ;'
          },
          {
            'start_line' => 18,
            'indent' => 0,
            'src' => ' plan tests => 3 ;',
            'block_id' => 0,
            'token_num' => 5,
            'end_line' => 18,
            'has_warnings' => 1
          },
          {
            'indent' => 0,
            'src' => ' use strict ;',
            'start_line' => 19,
            'end_line' => 19,
            'has_warnings' => 0,
            'block_id' => 0,
            'token_num' => 3
          },
          {
            'has_warnings' => 0,
            'end_line' => 24,
            'token_num' => 12,
            'block_id' => 0,
            'start_line' => 21,
            'src' => ' my ( @body ) = ( "<mailto:xxxx.xxxx\\@outlook.com>" , "A\\x{B9}ker\\x{E8}eva xxxx.xxxx\\@outlook.com \\x{201D}" , ) ;',
            'indent' => 0
          },
          {
            'end_line' => 31,
            'has_warnings' => 1,
            'block_id' => 0,
            'token_num' => 27,
            'indent' => 0,
            'src' => ' for ( @body ) { s{ <? (?<!mailto:) \\b ( [a-z0-9.]+ \\@ \\S+ ) \\b
     (?: > | \\s{1,10} (?!phone) [a-z]{2,11} : ) }{ }xgi ; my $got = $1 ; is ( $got , \'.xxxx@outlook.com\' ) ; }',
            'start_line' => 26
          },
          {
            'start_line' => 27,
            'indent' => 1,
            'src' => ' s{ <? (?<!mailto:) \\b ( [a-z0-9.]+ \\@ \\S+ ) \\b
     (?: > | \\s{1,10} (?!phone) [a-z]{2,11} : ) }{ }xgi ;',
            'has_warnings' => 0,
            'end_line' => 28,
            'block_id' => 3,
            'token_num' => 9
          },
          {
            'token_num' => 5,
            'block_id' => 3,
            'end_line' => 29,
            'has_warnings' => 0,
            'start_line' => 29,
            'indent' => 1,
            'src' => ' my $got = $1 ;'
          },
          {
            'has_warnings' => 1,
            'end_line' => 30,
            'block_id' => 3,
            'token_num' => 7,
            'src' => ' is ( $got , \'.xxxx@outlook.com\' ) ;',
            'indent' => 1,
            'start_line' => 30
          },
          {
            'start_line' => 32,
            'indent' => 0,
            'src' => ' ok ( "got to the end without dieing (note without DEBUGGING passing this test means nothing)" ) ;',
            'end_line' => 32,
            'has_warnings' => 1,
            'token_num' => 5,
            'block_id' => 0
          }
        ]
, 'Compiler::Lexer::get_groups_by_syntax_level');
};

subtest 'get_used_modules' => sub {
    my $modules = Compiler::Lexer->new('')->get_used_modules($script);
    is_deeply($modules, [
          {
            'name' => 'strict',
            'args' => ''
          },
          {
            'args' => '',
            'name' => 'warnings'
          },
          {
            'name' => 'strict',
            'args' => ''
          }
        ]
, 'Compiler::Lexer::get_used_modules');
};

done_testing;
