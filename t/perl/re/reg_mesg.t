subtest 'get_groups_by_syntax_level' => sub {
    my $lexer = Compiler::Lexer->new('');
    my $tokens = $lexer->tokenize($script);
    my $stmts = $lexer->get_groups_by_syntax_level($tokens, Compiler::Lexer::SyntaxType::T_Stmt);
    is_deeply($stmts, [
          {
            'start_line' => 3,
            'indent' => 0,
            'block_id' => 0,
            'src' => ' $| = 1 ;',
            'token_num' => 4,
            'end_line' => 3,
            'has_warnings' => 0
          },
          {
            'token_num' => 6,
            'src' => ' chdir \'t\' if -d \'t\' ;',
            'block_id' => 1,
            'start_line' => 6,
            'indent' => 1,
            'has_warnings' => 0,
            'end_line' => 6
          },
          {
            'token_num' => 5,
            'src' => ' @INC = qw \'../lib ../ext/re\' ;',
            'block_id' => 1,
            'start_line' => 7,
            'indent' => 1,
            'has_warnings' => 0,
            'end_line' => 7
          },
          {
            'src' => ' require \'./test.pl\' ;',
            'token_num' => 3,
            'start_line' => 8,
            'block_id' => 1,
            'indent' => 1,
            'has_warnings' => 0,
            'end_line' => 8
          },
          {
            'token_num' => 4,
            'src' => ' skip_all_without_unicode_tables ( ) ;',
            'indent' => 1,
            'start_line' => 9,
            'block_id' => 1,
            'has_warnings' => 1,
            'end_line' => 9
          },
          {
            'end_line' => 10,
            'has_warnings' => 0,
            'block_id' => 1,
            'start_line' => 10,
            'indent' => 1,
            'src' => ' eval \'require Config\' ;',
            'token_num' => 3
          },
          {
            'indent' => 0,
            'start_line' => 13,
            'block_id' => 0,
            'token_num' => 3,
            'src' => ' use strict ;',
