          {
            'args' => '',
            'name' => 'strict'
          },
          {
            'name' => 'open',
            'args' => '  qw ( :utf8 :std )'
          },
          {
            'args' => '',
            'name' => 'utf8'
          }
        ]
, 'Compiler::Lexer::get_used_modules');
};

