            'has_warnings' => 0
          },
          {
            'has_warnings' => 0,
            'end_line' => 14,
            'src' => ' use open qw(:utf8 :std) ;',
            'token_num' => 7,
            'start_line' => 14,
            'block_id' => 0,
            'indent' => 0
          },
          {
            'has_warnings' => 1,
            'end_line' => 45,
            'src' => ' sub fixup_expect { my $expect_ref = shift ; return "" if $expect_ref eq "" ; my @expect ; if ( ref $expect_ref ) { @expect = @$expect_ref ; } else { @expect = $expect_ref ; } foreach my $element ( @expect ) { $element =~ s/{\\#}/in regex; marked by <-- HERE in/ ; $element =~ s/{\\#}/ <-- HERE / ; $element .= " at " ; } return wantarray ? @expect : join "" , @expect ; }',
