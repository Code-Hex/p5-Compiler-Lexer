subtest 'get_groups_by_syntax_level' => sub {
    my $lexer = Compiler::Lexer->new('');
    my $tokens = $lexer->tokenize($script);
    my $stmts = $lexer->get_groups_by_syntax_level($tokens, Compiler::Lexer::SyntaxType::T_Stmt);
    is_deeply($stmts, [
          {
            'src' => ' use strict ;',
            'indent' => 0,
            'start_line' => 3,
            'end_line' => 3,
            'has_warnings' => 0,
            'block_id' => 0,
            'token_num' => 3
          },
          {
            'start_line' => 4,
            'indent' => 0,
            'src' => ' use warnings ;',
            'token_num' => 3,
            'block_id' => 0,
            'end_line' => 4,
            'has_warnings' => 0
          },
          {
            'src' => ' sub ok ( $$ ) { print "not " if ! $_ [ 0 ] ; print "ok" ; print " - $_[1]" if defined $_ [ 1 ] ; print "\\n" ; }',
            'indent' => 0,
            'start_line' => 14,
            'block_id' => 0,
            'token_num' => 31,
            'end_line' => 19,
            'has_warnings' => 0
          },
          {
            'has_warnings' => 0,
            'end_line' => 15,
            'block_id' => 2,
            'token_num' => 9,
            'start_line' => 15,
            'indent' => 1,
            'src' => ' print "not " if ! $_ [ 0 ] ;'
          },
          {
            'end_line' => 16,
            'has_warnings' => 0,
            'block_id' => 2,
            'token_num' => 3,
            'start_line' => 16,
            'indent' => 1,
            'src' => ' print "ok" ;'
          },
          {
            'end_line' => 17,
            'has_warnings' => 0,
            'token_num' => 9,
            'block_id' => 2,
            'start_line' => 17,
            'src' => ' print " - $_[1]" if defined $_ [ 1 ] ;',
            'indent' => 1
          },
          {
            'src' => ' print "\\n" ;',
            'indent' => 1,
            'start_line' => 18,
            'block_id' => 2,
            'token_num' => 3,
            'end_line' => 18,
            'has_warnings' => 0
          },
          {
            'token_num' => 9,
            'block_id' => 3,
            'end_line' => 22,
            'has_warnings' => 0,
            'src' => ' ok ${ ^GLOBAL_PHASE } eq \'START\' , \'START\' ;',
            'indent' => 1,
            'start_line' => 22
          },
          {
            'start_line' => 26,
            'src' => ' ok ${ ^GLOBAL_PHASE } eq \'CHECK\' , \'CHECK\' ;',
            'indent' => 1,
            'end_line' => 26,
            'has_warnings' => 0,
            'token_num' => 9,
            'block_id' => 4
          },
          {
            'end_line' => 30,
            'has_warnings' => 0,
            'block_id' => 5,
            'token_num' => 9,
            'start_line' => 30,
            'src' => ' ok ${ ^GLOBAL_PHASE } eq \'INIT\' , \'INIT\' ;',
            'indent' => 1
          },
          {
            'start_line' => 33,
            'src' => ' ok ${ ^GLOBAL_PHASE } eq \'RUN\' , \'RUN\' ;',
            'indent' => 0,
            'token_num' => 9,
            'block_id' => 0,
            'end_line' => 33,
            'has_warnings' => 0
          },
          {
            'indent' => 1,
            'src' => ' ok ${ ^GLOBAL_PHASE } eq \'RUN\' , \'DESTROY is run-time too, usually\' ;',
            'start_line' => 36,
            'block_id' => 6,
            'token_num' => 9,
            'has_warnings' => 0,
            'end_line' => 36
          },
          {
            'indent' => 1,
            'src' => ' ok ${ ^GLOBAL_PHASE } eq \'DESTRUCT\' , \'DESTRUCT\' ;',
            'start_line' => 42,
            'has_warnings' => 0,
            'end_line' => 42,
            'block_id' => 7,
            'token_num' => 9
          },
          {
            'token_num' => 9,
            'block_id' => 8,
            'has_warnings' => 0,
            'end_line' => 48,
            'start_line' => 48,
            'indent' => 1,
            'src' => ' ok ${ ^GLOBAL_PHASE } eq \'END\' , \'END\' ;'
          }
        ]
, 'Compiler::Lexer::get_groups_by_syntax_level');
};

subtest 'get_used_modules' => sub {
    my $modules = Compiler::Lexer->new('')->get_used_modules($script);
    is_deeply($modules, [
          {
            'name' => 'strict',
            'args' => ''
          },
          {
            'name' => 'warnings',
            'args' => ''
          }
        ]
, 'Compiler::Lexer::get_used_modules');
};

