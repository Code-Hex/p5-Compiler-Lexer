subtest 'get_groups_by_syntax_level' => sub {
    my $lexer = Compiler::Lexer->new('');
    my $tokens = $lexer->tokenize($script);
    my $stmts = $lexer->get_groups_by_syntax_level($tokens, Compiler::Lexer::SyntaxType::T_Stmt);
    is_deeply($stmts, [
          {
            'end_line' => 6,
            'has_warnings' => 0,
            'start_line' => 6,
            'block_id' => 1,
            'indent' => 1,
            'token_num' => 6,
            'src' => ' chdir \'t\' if -d \'t\' ;'
          },
          {
            'block_id' => 1,
            'start_line' => 7,
            'indent' => 1,
            'src' => ' require \'./test.pl\' ;',
            'token_num' => 3,
            'end_line' => 7,
            'has_warnings' => 0
          },
          {
            'token_num' => 7,
            'src' => ' plan ( tests => 5 ) ;',
            'start_line' => 10,
            'indent' => 0,
            'block_id' => 0,
            'has_warnings' => 1,
            'end_line' => 10
          },
          {
            'indent' => 0,
            'start_line' => 12,
            'block_id' => 0,
            'token_num' => 7,
            'src' => ' use open qw( :utf8 :std ) ;',
            'end_line' => 12,
            'has_warnings' => 0
          },
          {
            'token_num' => 4,
            'src' => ' use feature \'unicode_eval\' ;',
            'block_id' => 0,
            'start_line' => 13,
            'indent' => 0,
            'has_warnings' => 0,
            'end_line' => 13
          },
          {
            'indent' => 0,
            'start_line' => 15,
            'block_id' => 0,
            'src' => ' { my $w ; $SIG { __WARN__ } = sub { $w = shift } ; use utf8 ; my $prog = "qq!\\x{f9}!" ; eval $prog ; ok ! $w ; $w = "" ; utf8::upgrade ( $prog ) ; eval $prog ; is $w , \'\' ; }',
            'token_num' => 49,
            'end_line' => 28,
            'has_warnings' => 1
          },
          {
            'end_line' => 16,
            'has_warnings' => 0,
            'block_id' => 2,
            'start_line' => 16,
            'indent' => 1,
            'src' => ' my $w ;',
            'token_num' => 3
          },
          {
            'has_warnings' => 1,
            'end_line' => 17,
            'src' => ' $SIG { __WARN__ } = sub { $w = shift } ;',
            'token_num' => 12,
            'block_id' => 2,
            'start_line' => 17,
