subtest 'get_used_modules' => sub {
    my $modules = Compiler::Lexer->new('')->get_used_modules($script);
    is_deeply($modules, [
          {
            'args' => '',
            'name' => 'utf8'
          },
          {
            'args' => '  qw (  :utf8 :std  )',
            'name' => 'open'
          },
          {
            'name' => 'warnings',
            'args' => ''
          },
          {
            'args' => '  qw  \'unicode_strings evalbytes\'',
            'name' => 'feature'
          },
          {
            'name' => 'charnames',
            'args' => '  qw (  :full  )'
          },
          {
            'args' => '  \'unicode_eval\'',
            'name' => 'feature'
          }
        ]
, 'Compiler::Lexer::get_used_modules');
};

