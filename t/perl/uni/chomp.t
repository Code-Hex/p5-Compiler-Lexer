          },
          {
            'token_num' => 9,
            'has_warnings' => 1,
            'end_line' => 6,
            'src' => ' skip_all ( "EBCDIC" ) if $: : IS_EBCDIC ;',
            'start_line' => 6,
            'indent' => 1,
            'block_id' => 1
          },
          {
            'token_num' => 4,
            'has_warnings' => 1,
            'end_line' => 7,
            'src' => ' skip_all_without_perlio ( ) ;',
            'start_line' => 7,
            'indent' => 1,
            'block_id' => 1
          },
          {
            'token_num' => 3,
            'has_warnings' => 0,
            'end_line' => 10,
            'src' => ' use strict ;',
            'start_line' => 10,
            'indent' => 0,
            'block_id' => 0
          },
          {
            'token_num' => 3,
            'has_warnings' => 0,
            'end_line' => 11,
            'src' => ' use Encode ;',
            'start_line' => 11,
            'indent' => 0,
            'block_id' => 0
          },
          {
            'token_num' => 90,
            'has_warnings' => 1,
            'end_line' => 28,
            'src' => ' our %mbchars = ( \'big-5\' => { pack ( \'C*\' , 0x40 ) => pack ( \'U*\' , 0x40 ) , pack ( \'C*\' , 0xA4 , 0x40 ) => "\\x{4E00}" , } , \'euc-jp\' => { pack ( \'C*\' , 0xB0 , 0xA1 ) => "\\x{4E9C}" , pack ( \'C*\' , 0x8F , 0xB0 , 0xA1 ) => "\\x{4E02}" , } , \'shift-jis\' => { pack ( \'C*\' , 0xA9 ) => "\\x{FF69}" , pack ( \'C*\' , 0x82 , 0xA9 ) => "\\x{304B}" , } , ) ;',
            'start_line' => 15,
