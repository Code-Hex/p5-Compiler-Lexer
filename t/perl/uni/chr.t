subtest 'get_groups_by_syntax_level' => sub {
    my $lexer = Compiler::Lexer->new('');
    my $tokens = $lexer->tokenize($script);
    my $stmts = $lexer->get_groups_by_syntax_level($tokens, Compiler::Lexer::SyntaxType::T_Stmt);
    is_deeply($stmts, [
          {
            'src' => ' chdir \'t\' if -d \'t\' ;',
            'token_num' => 6,
            'start_line' => 4,
            'block_id' => 1,
            'indent' => 1,
            'has_warnings' => 0,
            'end_line' => 4
          },
          {
            'indent' => 1,
            'start_line' => 5,
            'block_id' => 1,
            'token_num' => 3,
            'src' => ' require \'./test.pl\' ;',
            'end_line' => 5,
            'has_warnings' => 0
          },
          {
            'has_warnings' => 1,
            'end_line' => 6,
            'token_num' => 5,
            'src' => ' skip_all_without_dynamic_extension ( \'Encode\' ) ;',
            'start_line' => 6,
            'block_id' => 1,
            'indent' => 1
          },
          {
            'token_num' => 9,
            'src' => ' skip_all ( "no encoding pragma in EBCDIC" ) if $: : IS_EBCDIC ;',
            'block_id' => 1,
            'start_line' => 7,
            'indent' => 1,
            'has_warnings' => 1,
            'end_line' => 7
          },
          {
            'end_line' => 8,
            'has_warnings' => 1,
            'start_line' => 8,
            'block_id' => 1,
            'indent' => 1,
            'token_num' => 4,
            'src' => ' skip_all_without_perlio ( ) ;'
          },
          {
            'has_warnings' => 0,
            'end_line' => 11,
            'src' => ' use strict ;',
            'token_num' => 3,
            'indent' => 0,
            'start_line' => 11,
            'block_id' => 0
          },
          {
            'token_num' => 7,
            'src' => ' plan ( tests => 8 ) ;',
            'start_line' => 12,
            'block_id' => 0,
            'indent' => 0,
            'has_warnings' => 1,
            'end_line' => 12
          },
          {
            'start_line' => 13,
            'block_id' => 0,
            'indent' => 0,
            'src' => ' no warnings \'deprecated\' ;',
            'token_num' => 4,
            'end_line' => 13,
            'has_warnings' => 1
          },
          {
            'has_warnings' => 0,
            'end_line' => 14,
            'token_num' => 4,
            'src' => ' use encoding \'johab\' ;',
            'start_line' => 14,
            'block_id' => 0,
            'indent' => 0
          },
          {
            'src' => ' ok ( chr ( 0x7f ) eq "\\x7f" ) ;',
            'token_num' => 10,
            'indent' => 0,
            'start_line' => 16,
            'block_id' => 0,
            'has_warnings' => 1,
            'end_line' => 16
          },
          {
            'end_line' => 17,
            'has_warnings' => 1,
            'start_line' => 17,
            'block_id' => 0,
            'indent' => 0,
            'src' => ' ok ( chr ( 0x80 ) eq "\\x80" ) ;',
            'token_num' => 10
          },
          {
            'block_id' => 0,
            'start_line' => 18,
            'indent' => 0,
            'src' => ' ok ( chr ( 0xff ) eq "\\xff" ) ;',
            'token_num' => 10,
            'end_line' => 18,
            'has_warnings' => 1
          },
          {
            'token_num' => 27,
            'src' => ' for my $i ( 127 , 128 , 255 ) { ok ( chr ( $i ) eq pack ( \'C\' , $i ) ) ; }',
            'indent' => 0,
            'start_line' => 20,
            'block_id' => 0,
            'has_warnings' => 1,
            'end_line' => 22
          },
          {
            'end_line' => 21,
            'has_warnings' => 1,
            'start_line' => 21,
            'block_id' => 2,
            'indent' => 1,
            'token_num' => 15,
            'src' => ' ok ( chr ( $i ) eq pack ( \'C\' , $i ) ) ;'
          },
          {
            'end_line' => 31,
            'has_warnings' => 1,
            'start_line' => 25,
            'indent' => 0,
            'block_id' => 0,
            'src' => ' { my $w ; local $SIG { __WARN__ } = sub { $w .= $_ [ 0 ] } ; my $chr = chr ( -1 ) ; is ( $chr , "\\x{fffd}" , "invalid values become REPLACEMENT CHARACTER" ) ; like ( $w , qr/^Invalid negative number \\(-1\\) in chr at / , "with a warning" ) ; }',
            'token_num' => 50
          },
          {
            'block_id' => 3,
            'start_line' => 26,
            'indent' => 1,
            'src' => ' my $w ;',
            'token_num' => 3,
            'end_line' => 26,
            'has_warnings' => 0
          },
          {
            'has_warnings' => 1,
            'end_line' => 27,
            'src' => ' local $SIG { __WARN__ } = sub { $w .= $_ [ 0 ] } ;',
            'token_num' => 16,
            'block_id' => 3,
            'start_line' => 27,
            'indent' => 1
          },
          {
            'start_line' => 28,
            'indent' => 1,
            'block_id' => 3,
            'token_num' => 8,
            'src' => ' my $chr = chr ( -1 ) ;',
            'end_line' => 28,
            'has_warnings' => 0
          },
          {
            'token_num' => 9,
            'src' => ' is ( $chr , "\\x{fffd}" , "invalid values become REPLACEMENT CHARACTER" ) ;',
            'start_line' => 29,
            'indent' => 1,
            'block_id' => 3,
            'has_warnings' => 1,
            'end_line' => 29
          },
          {
            'end_line' => 30,
            'has_warnings' => 1,
            'start_line' => 30,
            'indent' => 1,
            'block_id' => 3,
            'src' => ' like ( $w , qr/^Invalid negative number \\(-1\\) in chr at / , "with a warning" ) ;',
            'token_num' => 12
          }
        ]
, 'Compiler::Lexer::get_groups_by_syntax_level');
};

subtest 'get_used_modules' => sub {
    my $modules = Compiler::Lexer->new('')->get_used_modules($script);
    is_deeply($modules, [
          {
            'args' => '',
            'name' => 'strict'
          },
          {
            'args' => '  \'johab\'',
            'name' => 'encoding'
          }
        ]
, 'Compiler::Lexer::get_used_modules');
};

