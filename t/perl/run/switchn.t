subtest 'get_groups_by_syntax_level' => sub {
    my $lexer = Compiler::Lexer->new('');
    my $tokens = $lexer->tokenize($script);
    my $stmts = $lexer->get_groups_by_syntax_level($tokens, Compiler::Lexer::SyntaxType::T_Stmt);
    is_deeply($stmts, [
          {
            'src' => ' chdir \'t\' if -d \'t\' ;',
            'indent' => 1,
            'start_line' => 4,
            'token_num' => 6,
            'block_id' => 1,
            'has_warnings' => 0,
            'end_line' => 4
          },
          {
            'token_num' => 4,
            'block_id' => 1,
            'end_line' => 5,
            'has_warnings' => 0,
            'start_line' => 5,
            'src' => ' @INC = \'../lib\' ;',
            'indent' => 1
          },
          {
            'start_line' => 6,
            'indent' => 1,
            'src' => ' require \'./test.pl\' ;',
            'token_num' => 3,
            'block_id' => 1,
            'has_warnings' => 0,
            'end_line' => 6
          },
          {
            'src' => ' *ARGV = *DATA ;',
            'indent' => 1,
            'start_line' => 7,
            'has_warnings' => 0,
            'end_line' => 7,
            'token_num' => 4,
            'block_id' => 1
          },
          {
            'token_num' => 7,
            'block_id' => 1,
            'end_line' => 8,
            'has_warnings' => 1,
            'indent' => 1,
            'src' => ' plan ( tests => 3 ) ;',
            'start_line' => 8
          },
          {
            'block_id' => 2,
            'token_num' => 5,
            'has_warnings' => 1,
            'end_line' => 12,
            'src' => ' pass ( "Final test" ) ;',
            'indent' => 1,
            'start_line' => 12
          },
          {
            'start_line' => 15,
            'src' => ' chomp ;',
            'indent' => 0,
            'has_warnings' => 0,
            'end_line' => 15,
            'block_id' => 0,
            'token_num' => 2
          },
          {
            'indent' => 0,
            'src' => ' is ( "ok " . $. , $_ , "Checking line $." ) ;',
            'start_line' => 16,
            'has_warnings' => 1,
            'end_line' => 16,
            'block_id' => 0,
            'token_num' => 11
          },
          {
            'start_line' => 18,
            'src' => ' s/^/not / ;',
            'indent' => 0,
            'end_line' => 18,
            'has_warnings' => 0,
            'token_num' => 7,
            'block_id' => 0
          }
        ]
, 'Compiler::Lexer::get_groups_by_syntax_level');
};

