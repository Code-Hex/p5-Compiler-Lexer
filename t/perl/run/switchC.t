subtest 'get_groups_by_syntax_level' => sub {
    my $lexer = Compiler::Lexer->new('');
    my $tokens = $lexer->tokenize($script);
    my $stmts = $lexer->get_groups_by_syntax_level($tokens, Compiler::Lexer::SyntaxType::T_Stmt);
    is_deeply($stmts, [
          {
            'has_warnings' => 0,
            'end_line' => 6,
            'token_num' => 6,
            'src' => ' chdir \'t\' if -d \'t\' ;',
            'start_line' => 6,
            'block_id' => 1,
            'indent' => 1
          },
          {
            'end_line' => 7,
            'has_warnings' => 0,
            'start_line' => 7,
            'block_id' => 1,
            'indent' => 1,
            'src' => ' @INC = \'../lib\' ;',
            'token_num' => 4
          },
          {
            'token_num' => 3,
            'src' => ' require "./test.pl" ;',
            'start_line' => 8,
            'indent' => 1,
            'block_id' => 1,
            'has_warnings' => 0,
            'end_line' => 8
          },
          {
            'end_line' => 10,
            'has_warnings' => 1,
            'indent' => 1,
            'start_line' => 10,
            'block_id' => 1,
            'src' => ' skip_all_without_perlio ( ) ;',
            'token_num' => 4
          },
          {
            'end_line' => 11,
            'has_warnings' => 1,
            'block_id' => 1,
            'start_line' => 11,
            'indent' => 1,
            'token_num' => 5,
            'src' => ' skip_all_if_miniperl ( \'-C and $ENV{PERL_UNICODE} are disabled on miniperl\' ) ;'
          },
          {
            'end_line' => 14,
            'has_warnings' => 1,
            'indent' => 0,
            'start_line' => 14,
            'block_id' => 0,
            'token_num' => 7,
            'src' => ' plan ( tests => 15 ) ;'
          },
          {
            'indent' => 0,
            'start_line' => 16,
            'block_id' => 0,
            'token_num' => 3,
            'src' => ' my $r ;',
            'end_line' => 16,
            'has_warnings' => 0
          },
          {
            'src' => ' my $tmpfile = tempfile ( ) ;',
            'token_num' => 7,
            'indent' => 0,
            'start_line' => 18,
            'block_id' => 0,
            'has_warnings' => 1,
            'end_line' => 18
          },
          {
            'src' => ' my $scriptfile = tempfile ( ) ;',
            'token_num' => 7,
            'start_line' => 19,
            'block_id' => 0,
            'indent' => 0,
            'has_warnings' => 1,
            'end_line' => 19
          },
          {
            'has_warnings' => 0,
            'end_line' => 21,
            'src' => ' my $b = chr 256 ;',
            'token_num' => 6,
            'start_line' => 21,
            'block_id' => 0,
            'indent' => 0
          },
          {
            'end_line' => 21,
            'has_warnings' => 1,
            'block_id' => 0,
            'start_line' => 21,
            'indent' => 0,
            'src' => ' utf8::encode $b ;',
            'token_num' => 3
          },
          {
            'end_line' => 25,
            'has_warnings' => 1,
            'block_id' => 0,
            'start_line' => 23,
            'indent' => 0,
            'token_num' => 21,
            'src' => ' $r = runperl ( switches => [ \'-CO\' , \'-w\' ] , prog => \'print chr(256)\' , stderr => 1 ) ;'
          },
          {
            'start_line' => 26,
            'indent' => 0,
            'block_id' => 0,
            'token_num' => 13,
            'src' => ' like ( $r , qr/^$b(?:\\r?\\n)?$/s , \'-CO: no warning on UTF-8 output\' ) ;',
            'end_line' => 26,
            'has_warnings' => 1
          },
          {
            'token_num' => 21,
            'src' => ' $r = runperl ( switches => [ \'-C2\' , \'-w\' ] , prog => \'print chr(256)\' , stderr => 1 ) ;',
            'start_line' => 28,
            'indent' => 0,
            'block_id' => 0,
            'has_warnings' => 1,
            'end_line' => 30
          },
          {
            'end_line' => 31,
            'has_warnings' => 1,
            'start_line' => 31,
            'block_id' => 0,
            'indent' => 0,
            'src' => ' like ( $r , qr/^$b(?:\\r?\\n)?$/s , \'-C2: no warning on UTF-8 output\' ) ;',
            'token_num' => 13
          },
          {
            'has_warnings' => 1,
            'end_line' => 36,
            'src' => ' if ( exists $ENV { PERL_UNICODE } && ( $ENV { PERL_UNICODE } eq "" || $ENV { PERL_UNICODE } =~/[SO]/ ) ) { skip ( qq[cannot test with PERL_UNICODE "" or /[SO] / ] , 1 ) ;',
