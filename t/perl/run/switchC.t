            'start_line' => 34,
            'indent' => 1,
            'block_id' => 2
          },
          {
            'src' => ' if ( exists $ENV { PERL_UNICODE } && ( $ENV { PERL_UNICODE } eq "" || $ENV { PERL_UNICODE } =~/[SO]/ ) ) { skip ( qq[cannot test with PERL_UNICODE "" or /[SO] / ] , 1 )',
            'token_num' => 38,
            'start_line' => 34,
            'indent' => 1,
            'block_id' => 2,
            'has_warnings' => 1,
            'end_line' => 36
          },
          {
            'start_line' => 38,
            'block_id' => 0,
            'indent' => 0,
            'src' => ' $r = runperl ( switches => [ \'-CI\' , \'-w\' ] , prog => \'print ord(<STDIN>)\' , stderr => 1 , stdin => $b ) ;',
            'token_num' => 25,
            'end_line' => 41,
            'has_warnings' => 1
          },
          {
            'token_num' => 13,
            'src' => ' like ( $r , qr/^256(?:\\r?\\n)?$/s , \'-CI: read in UTF-8 input\' ) ;',
            'block_id' => 0,
            'start_line' => 42,
            'indent' => 0,
            'has_warnings' => 1,
            'end_line' => 42
          }
        ]
, 'Compiler::Lexer::get_groups_by_syntax_level');
};

