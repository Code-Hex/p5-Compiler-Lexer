subtest 'get_groups_by_syntax_level' => sub {
    my $lexer = Compiler::Lexer->new('');
    my $tokens = $lexer->tokenize($script);
    my $stmts = $lexer->get_groups_by_syntax_level($tokens, Compiler::Lexer::SyntaxType::T_Stmt);
    is_deeply($stmts, [
          {
            'has_warnings' => 0,
            'end_line' => 3,
            'src' => ' my $Perl ;',
            'token_num' => 3,
            'start_line' => 3,
            'block_id' => 0,
            'indent' => 0
          },
          {
            'start_line' => 4,
            'block_id' => 0,
            'indent' => 0,
            'token_num' => 3,
            'src' => ' my $dtrace ;',
            'end_line' => 4,
            'has_warnings' => 0
          },
          {
            'has_warnings' => 0,
            'end_line' => 7,
            'token_num' => 6,
            'src' => ' chdir \'t\' if -d \'t\' ;',
            'block_id' => 1,
            'start_line' => 7,
            'indent' => 1
          },
          {
            'end_line' => 8,
            'has_warnings' => 0,
            'indent' => 1,
            'start_line' => 8,
            'block_id' => 1,
            'src' => ' @INC = \'../lib\' ;',
            'token_num' => 4
          },
          {
            'has_warnings' => 0,
            'end_line' => 9,
            'token_num' => 3,
            'src' => ' require \'./test.pl\' ;',
            'block_id' => 1,
            'start_line' => 9,
            'indent' => 1
          },
          {
            'has_warnings' => 1,
            'end_line' => 11,
            'src' => ' skip_all_without_config ( "usedtrace" ) ;',
            'token_num' => 5,
            'start_line' => 11,
            'indent' => 1,
            'block_id' => 1
          },
          {
            'end_line' => 13,
            'has_warnings' => 1,
            'start_line' => 13,
            'indent' => 1,
            'block_id' => 1,
            'token_num' => 7,
            'src' => ' $dtrace = $Config::Config { dtrace } ;'
          },
          {
            'has_warnings' => 1,
            'end_line' => 15,
            'src' => ' $Perl = which_perl ( ) ;',
            'token_num' => 6,
            'indent' => 1,
            'start_line' => 15,
            'block_id' => 1
          },
          {
            'indent' => 1,
            'start_line' => 17,
            'block_id' => 1,
            'token_num' => 7,
            'src' => ' `$dtrace -V` or skip_all ( "$dtrace unavailable" ) ;',
            'end_line' => 17,
            'has_warnings' => 1
          },
          {
            'src' => ' my $result = `$dtrace -qnBEGIN -c\'$Perl -e 1\' 2>&1` ;',
            'token_num' => 5,
            'start_line' => 19,
